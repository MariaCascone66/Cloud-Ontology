  author={Hassan, Mohammad Mehedi and Albakr, Hanouf and Al-Dossari, Hmood and Mohamed, Amr},
  journal={IEEE Access}, 
  title={Resource Provisioning for Cloud-Assisted Body Area Network in a Smart Home Environment}, 
  year={2017},
  volume={5},
  number={},
  pages={13213-13224},
  abstract={In recent years, cloud-assisted body area network (CABAN) technologies have made their entrance in the Smart healthcare field, such as Smart home environment, and play a significant role for healthcare data storage, processing, and efficient decision making. However, currently, the CABAN paradigm in the healthcare domain is facing increasing difficulty in handling the huge amount of sensor data that the body sensor devices generate from diverse Smart home applications. Therefore, the challenging is now timely storing, processing, and analyzing of the sensor data in real time to maintain the Quality of Service (QoS) requirements of the caregivers or Smart home applications. QoS, here, is the capacity to support diverse Smart home applications in healthcare with different priorities, performance, and resource requirements. Therefore, in this paper, we present a fast and robust cloud resource allocation model for body sensor devices to ensure QoS for Smart home healthcare applications. We develop the proposed resource allocation algorithm using agent-based modeling (ABM) and ontology. There are few works, which consider ABM and ontology for resource allocation in CABAN platform. Moreover, we used an ABM tool called NetLogo to implement the proposed resource allocation model. The results from the implementation were compared with the results of existing algorithms and found to be promising.},
  keywords={Resource management;Cloud computing;Quality of service;Medical services;Smart homes;Body area networks;Algorithm design and analysis;Cloud-assisted body area network;QoS;resource allocation;agent-based modeling and ontology},
  doi={10.1109/ACCESS.2017.2726012},
  ISSN={2169-3536},
  month={},}@INPROCEEDINGS{7965749,
  author={Shao, Lixu and Duan, Yucong and Sun, Xiaobing and Zou, Quan and Jing, Rongqi and Lin, Jiami},
  booktitle={2017 IEEE 15th International Conference on Software Engineering Research, Management and Applications (SERA)}, 
  title={Bidirectional value driven design between economical planning and technical implementation based on data graph, information graph and knowledge graph}, 
  year={2017},
  volume={},
  number={},
  pages={339-344},
  abstract={Value-Driven Design enables rational decisions to be made in terms of the optimum business and technical solution at every level of engineering design by employing economics in decision making. In order to maximize the business profitability, we propose to bridge bidirectional value driven design between economic planning and technology implementation on the basis of the data graph, information graph and knowledge graph. We use data graph, information graph and knowledge graph to analyze problems that have negative impact on activities of software development including requirement analysis, summary design and detail design. We propose to improve system reliability and robustness by managing data and information reuse, redundancy as well as structure.},
  keywords={Software;Data models;Cognition;Planning;Stakeholders;Redundancy},
  doi={10.1109/SERA.2017.7965749},
