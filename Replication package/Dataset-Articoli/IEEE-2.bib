  year={2021},
  volume={},
  number={},
  pages={537-541},
  abstract={In the paper, we consider the use of a virtual assistant with a natural language interface for the task of population of the OWL ontology of computer equipment products based on the information provided on the websites of online stores. The practical value of the work is to provide the user with the opportunity to search for the required products simultaneously on several sites based on a search query described in natural language.},
  keywords={Virtual assistants;Natural languages;Sociology;OWL;Ontologies;Software systems;Data mining;virtual assistant;conversational agent;chatbot;ontology;ontological model;model-theoretical methods;knowledge extraction;named entity recognition},
  doi={10.1109/EDM52169.2021.9507641},
  ISSN={2325-419X},
  month={June},}@INPROCEEDINGS{9509914,
  author={Mahdi, Ahmed M. and Hadi, Asaad S.},
  booktitle={2021 1st Babylon International Conference on Information Technology and Science (BICITS)}, 
  title={Utilizing LOD Relationships and FOAF Vocabularies for Top-N Recommender System}, 
  year={2021},
  volume={},
  number={},
  pages={98-103},
  abstract={The World Wide Web is transitioning from a Web of hyper-linked documents to a Web of linked data. A large amount of Resource Description Framework (RDF) data was published in publicly available datasets and connected to create the so-called Linked Open Data cloud. The semantics embedded in the Linked Open Data (LOD) can be utilized to enhance the recommender systems (RSs). Limited content analysis, cold-start and data sparsity are well-known issues in traditional RSs, which occurs when few or no features describe the items or no ratings to achieve a recommendation task. Because the LOD cloud contains many features, the knowledge encoded can help resolve this issue. The proposed system's main idea is to design a knowledge-based recommendation system that uses semantic features extracted from multiple datasets in LOD. Our approach will generate recommendations depending on direct and indirect relationships between resources. The results will be ranked according to their similarity score with the input before presenting them to the user. The results of our approach were comparable to the results of the Internet Movie Database (IMDb) website. Furthermore, an experimental evaluation using the MovieLens dataset was conducted. The results were encouraging and stimulating further research in this particular field of study. The usage of different kinds of relations in LOD can enhance the accuracy of the recommendations.},
  keywords={Cloud computing;Vocabulary;Semantics;Feature extraction;Motion pictures;Resource description framework;Web sites;Linked Open Data;Recommender Systems;Semantic Web;RDF;FOAF;SPARQL;DBpedia;LinkedMDB},
  doi={10.1109/BICITS51482.2021.9509914},
  ISSN={},
  month={April},}@INPROCEEDINGS{9482574,
  author={Kukhtevich, Igor and Goryunova, Valentina and Goryunova, Tatyana},
  booktitle={2021 1st International Conference on Technology Enhanced Learning in Higher Education (TELE)}, 
  title={Aspects of Using Intelligent Cloud Technologies in Professional Training of General Practitioners}, 
  year={2021},
  volume={},
  number={},
  pages={53-57},
  abstract={Currently, there is a clear trend towards personalized medicine that will be most effective when using cloud technologies. With new digital services, there is also a growing need for cloud-based transformation of essential medical systems and applications to improve the quality of health worker education. The challenge for the near future is the development of cloud solutions into an intelligent physician assistant, including the introduction of integrated clinical applications, automatic quality control of medical care and other areas that can significantly improve the quality of medical care and increase the effectiveness of training. The article discusses the possibility of using cloud applications used by various organizations for professional retraining of general practitioners. A generalized methodology for the use of universal interfaces for clinical applications by general practitioners in the process of their professional retraining is presented. Aspects of standardization of medical information systems and the use of standard products, including ontologies in the creation of cloud learning environments, are noted. It also highlights the need for healthcare leaders to explore integrated solutions that include medical imaging, health care management and financial management. The article presents the possibility of using Predix cloud platform solutions for training general practitioners.},
  keywords={Training;Standards organizations;Medical services;Quality control;Tools;Ontologies;Medical information systems;cloud;healthcare;education and retraining;general practitioner;integrated clinical applications;intelligent systems},
  doi={10.1109/TELE52840.2021.9482574},
  ISSN={},
  month={June},}@INPROCEEDINGS{9463546,
  author={Kim, Dae-young and Joshi, Karuna P.},
  booktitle={2021 7th IEEE Intl Conference on Big Data Security on Cloud (BigDataSecurity), IEEE Intl Conference on High Performance and Smart Computing, (HPSC) and IEEE Intl Conference on Intelligent Data and Security (IDS)}, 
  title={A Semantically Rich Knowledge Graph to Automate HIPAA Regulations for Cloud Health IT Services}, 
  year={2021},
  volume={},
  number={},
  pages={7-12},
  abstract={As healthcare organizations adopt cloud-based services to manage their patient data, compliance with the rules and policies of the Health Insurance Portability and Accountability Act (HIPAA) regulation becomes increasingly complex. At present, HIPAA rules are available only in large textual format and require significant human effort to implement in the Health IT systems. Moreover, every change in the regulation, like the recent relaxation in telehealth policy due to the COVID-19 pandemic, has to be manually implemented in the IT system. We have developed a semantically rich Knowledge graph, using Semantic Web technologies to represent HIPAA rules in a machine-processable format. This will significantly help in automatically reasoning of HIPAA policies. In this paper, we describe our design along with the results of our study of the current status of research on HIPAA ontology. We have validated our design against use cases defined by the US Department of Health and Human Services (HHS). This knowledge graph can be integrated with existing healthcare systems to provide automated compliance with HIPAA policies.},
  keywords={Knowledge engineering;Semantic Web;Privacy;US Department of Health and Human Services;Conferences;Medical services;Ontologies;HIPAA;Compliance;Knowledge Graph (Ontol-ogy);Semantic Web;Policy Expressions},
  doi={10.1109/BigDataSecurityHPSCIDS52275.2021.00013},
  ISSN={},
  month={May},}@INPROCEEDINGS{9463563,
  author={Gao, Yihang and Su, Peng and Zhao, Hui and Qiu, Meikang and Liu, Meiqin},
  booktitle={2021 7th IEEE Intl Conference on Big Data Security on Cloud (BigDataSecurity), IEEE Intl Conference on High Performance and Smart Computing, (HPSC) and IEEE Intl Conference on Intelligent Data and Security (IDS)}, 
  title={Research on Sentiment Dictionary Based on Sentiment Analysis in News Domain}, 
  year={2021},
  volume={},
  number={},
  pages={117-122},
  abstract={The known Chinese sentiment dictionary has universal applicability, lacks the expansion of proprietary domains, and lacks support for network words. This paper proposes a new approach and applies it to the Internet news sentiment analysis text provided by the CCF Big Data & Computing Intelligence Contest. First, the corpus is segmented in Chinese to obtain candidate sentiment words that have not appeared in the sentiment dictionary. The candidate sentiment words are calculated in different sentimental texts. The proportion of appearance is obtained, and the emotional tendency of candidate emotional words is obtained, and then it is merged with the emotional vocabulary ontology database to obtain the expanded emotional dictionary. Experiments show that the expanded sentiment dictionary is better than the known Chinese sentiment vocabulary ontology database in the above corpus.},
  keywords={Vocabulary;Sentiment analysis;Cloud computing;Dictionaries;Databases;Conferences;Natural languages;Natural Language Processing;Sentiment Analysis;Sentiment Dictionary;Internet News;SVM;Naïve Bayesian},
  doi={10.1109/BigDataSecurityHPSCIDS52275.2021.00031},
  ISSN={},
  month={May},}@INPROCEEDINGS{9452984,
  author={Huang, Xinping},
  booktitle={2021 5th International Conference on Trends in Electronics and Informatics (ICOEI)}, 
  title={Research on Ontology and Linked Data-oriented Construction of Government Website Web Archive Thematic Knowledge Base}, 
  year={2021},
  volume={},
  number={},
  pages={928-934},
  abstract={This research aims to explore the application of semantic web technologies such as ontology and linked data in the preservation of government website information resources. By constructing a government website Web Archive thematic knowledge base driven by semantic web technology, it can improve the historical traceability and data mining in the longterm preservation environment. The utilization rate and service quality of the original information of government websites with multiple utilization values such as reference for decision-making. On the basis of summarizing relevant research systems at home and abroad, this paper designs the special knowledge base of the government website Web Archive from three levels, namely, requirement analysis of the knowledge base system, conceptual structure design and logical structure design, and refers to the hierarchical idea of the OAIS model. And the Web Archive thematic knowledge base model for government websites based on ontology and associated data is constructed. Finally, the core process and key technology of the construction of the government website Web Archive thematic knowledge base are proposed, in order to provide reference and reference for research on longterm preservation of government website information resources.},
  keywords={Semantic Web;Analytical models;Systematics;Government;Knowledge based systems;Information services;Ontologies;Government Website;Web Archive;Ontology;Linked Data;Thematic Knowledge Base},
  doi={10.1109/ICOEI51242.2021.9452984},
  ISSN={},
  month={June},}@INPROCEEDINGS{9442516,
  author={Li, Yifeng and Tan, Yuan and Zhou, Ming and Zeng, Guangjun and Chen, Zhe and Li, Dong and Feng, Qi},
  booktitle={2021 IEEE 6th International Conference on Cloud Computing and Big Data Analytics (ICCCBDA)}, 
  title={Research on the Knowledge Graph Construction for Flipped Classroom}, 
  year={2021},
  volume={},
  number={},
  pages={636-641},
  abstract={Knowledge graph is a storage form that uses graph models to store entities and the relationships between entities and entities. We have built a knowledge graph for flipped classrooms to assist teaching work. In the process of constructing the knowledge graph and using the knowledge graph for recommendation, we designed a paragraph recommendation algorithm that comprehensive considering the student performance, content of course and user evaluation, which is used for personalized recommendation after the user enters the text. This algorithm sorts paragraphs based on the values of the Recopara. The user feedback mechanism in the algorithm can make the recommended paragraphs content better and better. Compared with the recommendation algorithm based on text content, the recommendation algorithm based on Recopara can better adapt to the needs of flipped classroom teaching and have been praised by the users.},
  keywords={Cloud computing;Conferences;Education;Big Data;Online services;knowledge graph;flipped classroom;recommendation algorithm;education assistant systems},
  doi={10.1109/ICCCBDA51879.2021.9442516},
  ISSN={},
  month={April},}@INPROCEEDINGS{9442554,
  author={Zhou, Yuwei and Feng, Jianhong and Yang, Zhaohong and Wang, Hongyu},
  booktitle={2021 IEEE 6th International Conference on Cloud Computing and Big Data Analytics (ICCCBDA)}, 
  title={Overview of Knowledge Graph Completion Algorithms Based on Representation Learning}, 
  year={2021},
  volume={},
  number={},
  pages={222-228},
  abstract={In recent years, the KG (Knowledge graph), as a new generation of knowledge engineering technical representatives, has been developed vigorously. KG is a key step for artificial intelligence to move towards intelligence. Knowledge graph completion (KGC) is a key technology in the construction of KG. The aim is to complement the missing knowledge in the KG. With the development of the Internet, the scale of data increases exponentially, and the scale of KG is increasing day by day. It is very inefficient to make up the missing knowledge based on manual, so many algorithms of KGC have emerged. In this paper, some typical KGC algorithms based on representation learning are summarized and combed, which are mainly divided into tensor decomposition, neural network and translation transformation. The development direction and prospect of KGC in KG are discussed and prospected.},
  keywords={Knowledge engineering;Cloud computing;Tensors;Conferences;Neural networks;Knowledge based systems;Manuals;KG;presentation learning;KGC},
  doi={10.1109/ICCCBDA51879.2021.9442554},
