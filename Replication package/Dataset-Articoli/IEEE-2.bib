  booktitle={2022 4th International Conference on Industrial Artificial Intelligence (IAI)}, 
  title={Research Hotspots and Dynamic Analysis of Knowledge Spillover in Modern Service Industry Based on CITESPACE Big Data Analysis}, 
  year={2022},
  volume={},
  number={},
  pages={1-5},
  abstract={The rapid and sustainable development of economy and society requires us to abandon the previous development mode and carry out the optimization and adjustment of industrial structure. In this paper, we use Citespace software and knowledge graph visualization technology to analyze the core journals in the database in terms of keyword analysis and literature co-citation analysis based on the database information of WOS, with “modern service industry” and “knowledge spillover” as the themes. Through the visual analysis of the knowledge map, we comprehensively study and discuss the current status of research on “modern service industry” and “modern service industry” related to knowledge spillover, as well as the research hotspots and development trends in various periods, so as to prepare for the next step of “knowledge spillover in modern service industry”. It will pave the way for the next research on “knowledge spillover in modern service industry”.},
  keywords={Industries;Visualization;Technological innovation;Soft sensors;Big Data;Market research;Visual databases;Big data analysis;Knowledge spillover;Modern service industry},
  doi={10.1109/IAI55780.2022.9976611},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{9973961,
  author={Gorvunova, Valentina and Kukhtevich, Igor and Goryunova, Tatyana},
  booktitle={2022 4th International Conference on Control Systems, Mathematical Modeling, Automation and Energy Efficiency (SUMMA)}, 
  title={Digitalization and Integration Cloud Solutions for Healthcare Information Systems}, 
  year={2022},
  volume={},
  number={},
  pages={608-611},
  abstract={Infocommunication technological solutions in healthcare are an environment at the intersection of information technologies, achievements in medical science, the work of medical institutions, insurance companies, doctors and patients. The authors emphasize that in addition to this, there is a clear trend towards personalized medicine, which will be most effective when using cloud technologies. It is noted that medical organizations mainly use information systems that ensure the transfer of financial, operational and personnel applications to the cloud. With the advent of new digital services, there is also an increasing need for cloud transformation of key medical systems and applications to improve the quality of patient care. The article discusses aspects of the implementation of cloud infrastructure solutions in the process of automating various clinical areas. It highlights the need for integrated healthcare solutions including medical imaging and emergency medical care. The possibility of combining applications, servers and databases used by various organizations (health authorities, medical organizations, research centers, etc.) into a common network using modern cloud platform solutions is noted. The aspects of standardization of medical information systems and the use of standard products, including ontologies, are noted. The task of the near future is the development of cloud solutions in the intelligent assistant to the doctor, including the introduction of integrated clinical applications, automatic quality control of medical care and other areas. The possibility of developing universal interfaces for integrated clinical applications is being considered. This is especially important in emergency situations.},
  keywords={Web services;Precision medicine;Standards organizations;Sociology;Medical services;Quality control;Servers;cloud computing;healthcare;integrated clinical applications;physician},
  doi={10.1109/SUMMA57301.2022.9973961},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{9955168,
  author={Wang, Jimin and Han, Bin and Zhang, Lei and Yang, Jiang},
  booktitle={2022 International Conference on Machine Learning, Cloud Computing and Intelligent Mining (MLCCIM)}, 
  title={A Joint Model Based on Prior Knowledge for War Research Report Information Extraction}, 
  year={2022},
  volume={},
  number={},
  pages={79-84},
  abstract={War Research Reports (WRR) objectively describes the composition, deployment and cooperation of troops and weapons in the war, as well as the combat damage. WRR information extraction can provide data for the knowledge graph of damage. The War Research Report Dataset (WRRD) is constructed based on manual tagging and WRR collected on the Internet, then a joint entity and relation extraction model named PRK-CASREL based on prior knowledge for WRR is proposed. The result of the experiment indicates that shows that the proposed model has better recall and F1 than the models involved in comparison.},
  keywords={Cloud computing;Analytical models;Military computing;Weapons;Manuals;Machine learning;Tagging;Prior knowledge;Remote supervision;Joint extraction;Information extraction;War research report},
  doi={10.1109/MLCCIM55934.2022.00021},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{9942123,
  author={Luo, Wang and Lou, Chao and Xia, Yuan and Gao, De-Quan and Li, Ji-Wei and Zhao, Zi-Yan and Lai, Feng-Gang and Ma, Chao},
  booktitle={2022 Global Reliability and Prognostics and Health Management (PHM-Yantai)}, 
  title={Research on Entity Update Technology for Fault Diagnosis Knowledge Graph of Cloud Data Center}, 
  year={2022},
  volume={},
  number={},
  pages={1-6},
  abstract={The Fault Diagnosis Knowledge Graph (FDKG) of Cloud Data Center (CDC), in a broad sense, is the knowledge Digital Twin of the fault phenomenon, reasoning, and maintenance process of Cloud Data Center in the physical world. The key to the Digital Twin is to establish the information interface between the physical space and the virtual space, and the key to the construction of the fault diagnosis KG is also here. FDKG of The State Grid Cloud Data Center needs to integrate multi-source knowledge to establish an information interface with the CDCs of subsidiaries in each province. However, in the process of updating the FDKG, the entity name attribute represented by long sentences reduces the accuracy of entity alignment, and it is difficult to efficiently integrate knowledge into the FDKG without increasing knowledge redundancy. This paper proposes an entity alignment method based on the fusion of attribute and relationship similarity, which will use the clearly defined relationship information in the FDKG to effectively improve the accuracy of entity alignment. The knowledge update tool developed based on this, effectively improves the entity alignment accuracy of the FDKG, and improves the information interface connection efficiency of the FDKG of the CDC.},
  keywords={Fault diagnosis;Data centers;Cloud computing;Redundancy;Maintenance engineering;Cognition;Digital twins;Cloud Data Center;fault diagnosis;Knowledge Graph;Knowledge Update},
  doi={10.1109/PHM-Yantai55411.2022.9942123},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{9930262,
  author={Liu, Junju},
  booktitle={2022 IEEE 13th International Conference on Software Engineering and Service Science (ICSESS)}, 
  title={Textual and Graph Representation Learning-based Service Recommendation for Application Construction}, 
  year={2022},
  volume={},
  number={},
  pages={252-255},
  abstract={As a new computing paradigm, service-oriented computing has seen fast development in recent years. As a result, a proliferation of both traditional Web-based and cloud-based service models have appeared online. These open services will play a crucial role in developing applications. However, the difficulty of service selection in developing a service-oriented system grows in proportion to the growing number of available services. This research suggests expanding upon the foundation of the original collaborative filtering recommendation algorithm by developing a model for service recommendations based on the study of representation learning of knowledge graphs and texts. In this research, we focus on modeling the semantic features of applications and services through the use of Translating Embedding, a knowledge graph feature learning approach, and the textual features learned from topic models. We conducted experiments on a real-world dataset, and the results demonstrate that the suggested method is able to locate services that are pertinent to developer needs.},
  keywords={Representation learning;Knowledge engineering;Computational modeling;Collaborative filtering;Semantics;Software;Recommender systems;Knowledge Graph;Service-oriented System;Service recommendation},
  doi={10.1109/ICSESS54813.2022.9930262},
  ISSN={2327-0594},
  month={Oct},}@INPROCEEDINGS{9912259,
  author={Chen, Hongyin and Lu, Yuxuan and Cheng, Yukun},
  booktitle={2022 IEEE 42nd International Conference on Distributed Computing Systems (ICDCS)}, 
  title={FileInsurer: A Scalable and Reliable Protocol for Decentralized File Storage in Blockchain}, 
  year={2022},
  volume={},
  number={},
  pages={168-179},
  abstract={With the development of blockchain applications, the requirements for file storage in blockchain are increasing rapidly. Many protocols, including Filecoin, Arweave, and Sia, have been proposed to provide scalable decentralized file storage for blockchain applications. However, the reliability is not well promised by existing protocols. Inspired by the idea of insurance, we innovatively propose a decentralized file storage protocol in blockchain, named as FileInsurer, to achieve both scalability and reliability. While ensuring scalability by distributed storage, FileInsurer guarantees reliability by enhancing robustness and fully compensating for the file loss. Specifically, under mild conditions, we prove that no more than 0.1% value of all files should be compensated even if half of the storage collapses. Therefore, only a relatively small deposit needs to be pledged by storage providers to cover the potential file loss. Because of lower burdens of deposit, storage providers have more incentives to participate in the storage network. FileInsurer can run in the top layer of the InterPlanetary File System (IPFS), and thus it can be directly applied in Web 3.0, Non-Fungible Tokens, and Metaverse.},
  keywords={Semantic Web;Metaverse;Scalability;Insurance;Reliability engineering;Robustness;InterPlanetary File System;Decentralized File Storage;Blockchain application;Mechanism Design;Decentralized System;Insurance},
  doi={10.1109/ICDCS54860.2022.00025},
  ISSN={2575-8411},
  month={July},}@INPROCEEDINGS{9912170,
  author={Zahid, Muhammad Adeel and Shafiq, Basit and Shamail, Shafay and Afzal, Ayesha and Vaidya, Jaideep},
  booktitle={2022 IEEE 42nd International Conference on Distributed Computing Systems (ICDCS)}, 
  title={BP-DEBUG: A Fault Debugging and Resolution Tool for Business Processes}, 
  year={2022},
  volume={},
  number={},
  pages={1306-1309},
  abstract={Cloud computing and Internet-ware software paradigm have enabled rapid development of distributed business process (BP) applications. Several tools are available to facilitate automated/ semi-automated development and deployment of such distributed BPs by orchestrating relevant service components in a plug-and-play fashion. However, the BPs developed using such tools are not guaranteed to be fault-free. In this demonstration, we present a tool called BP-DEBUG for debugging and automated repair of faulty BPs. BP-DEBUG implements our Collaborative Fault Resolution (CFR) approach that utilizes the knowledge of existing BPs with a similar set of web services fault detection and resolution in a given user BP. Essentially, CFR attempts to determine any semantic and structural differences between a faulty BP and related BPs and computes a minimum set of transformations which can be used to repair the faulty BP. Demo url: https://youtu.be/mf49oSekLOA},
  keywords={Knowledge engineering;Web services;Fault detection;Semantics;Collaboration;Debugging;Maintenance engineering;Business process debugging;Business process repair;fault-resolution;Web services},
  doi={10.1109/ICDCS54860.2022.00143},
  ISSN={2575-8411},
  month={July},}@INPROCEEDINGS{9908022,
  author={Mitropoulou, Katerina and Kokkinos, Panagiotis and Soumplis, Polyzois and Varvarigos, Emmanouel},
  booktitle={2022 13th International Symposium on Communication Systems, Networks and Digital Signal Processing (CSNDSP)}, 
  title={Detect Resource Related Events in a Cloud-Edge Infrastructure using Knowledge Graph Embeddings and Machine Learning}, 
  year={2022},
  volume={},
  number={},
  pages={698-703},
  abstract={Edge and cloud computing infrastructures consist of multiple resources that may belong to different providers and are utilized in a shared manner by distributed applications for computing and storage purposes. Detecting events that affect the efficient operation of such infrastructures is a challenge and absolutely necessary for providing high quality cloud-edge services. In this work, we model cloud-edge infrastructures using knowledge graphs and use graph embeddings to transform the graphs into vectors. Then, traditional data-driven machine learning algorithms are used in order to detect anomaly events that relate to the infrastructure usage.},
  keywords={Cloud computing;Machine learning algorithms;Event detection;Image edge detection;Computational modeling;Simulation;Signal processing algorithms;Edge computing;cloud computing;event detection;Knowledge graph;graph embeddings;machine learning},
  doi={10.1109/CSNDSP54353.2022.9908022},
  ISSN={},
  month={July},}@INPROCEEDINGS{9895097,
  author={Charoensuk, Jirawan and Phansiri, Thaninrat and Abdulroman, Jessada and Chootong, Chalothon},
  booktitle={2022 37th International Technical Conference on Circuits/Systems, Computers and Communications (ITC-CSCC)}, 
  title={Development and Visualization of PS-Dict: a Programmer Skills Dictionary}, 
  year={2022},
  volume={},
  number={},
  pages={383-386},
  abstract={Programmers need to have a wide range of skills to perform their tasks. Using programming tools is not enough for purveying full functionality and development and thus should be combined with essential abilities such as problem-solving, along with in-depth knowledge about operating systems, hardware, and software. Hence, knowledge concerning these various aspects is important. In this study, we propose an interactive programmer skills dictionary in both English and Thai called PS-Dict. Our system produces a Knowledge Graphs using with the first paragraph from a Wikipedia Infobox. The comparison results show that PS-Dict could provide useful information in terms of skill descriptions and a list of related knowledge.},
  keywords={Visualization;Dictionaries;Operating systems;Encyclopedias;Programming;Software;Hardware;programmer skills;programmer dictionary;knowledge graph;programming language},
  doi={10.1109/ITC-CSCC55581.2022.9895097},
  ISSN={},
  month={July},}@INPROCEEDINGS{9904781,
  author={Roy, Souradip and Li, Juan and Pandey, Vikram and Bai, Yan},
  booktitle={2022 10th IEEE International Conference on Mobile Cloud Computing, Services, and Engineering (MobileCloud)}, 
  title={An Explainable Deep Neural Framework for Trustworthy Network Intrusion Detection}, 
  year={2022},
  volume={},
  number={},
  pages={25-30},
  abstract={In recent years, there has been an increase in cyber attacks in mobile cloud environment. Intrusion Detection Systems (IDS) have played an important role in protecting mobile cloud security. Many techniques have been utilized to implement IDS, among them, machine learning-based techniques have generated promising results. Especially, complex deep neural networks show a higher detection rate than traditional machine learning models. However, the interpretation of the decision made by a neural network becomes harder to understand as its architectural complexity increases. This challenge makes it difficult for the human experts to fine-tune their detection systems, trust the detection system’s results, and make decisions accordingly when IDS systems are deployed. To address this issue, we propose an explainable intrusion detection framework that employs deep learning mechanisms to identify cyber-attacks and utilizes knowledge graph as the knowledge foundation to add human understanding of machine learning and explain the machine learning results. The use case study demonstrates that the proposed framework can not only successfully identify network intrusions but also effectively reveal important information about its internal working mechanisms of the mysterious deep learning Blackbox.},
  keywords={Deep learning;Computational modeling;Cloud computing security;Neural networks;Network intrusion detection;Predictive models;Complexity theory;Explainable AI;Intrusion Detection;security;deep learning;knowledge graph},
  doi={10.1109/MobileCloud55333.2022.00011},
  ISSN={2573-7562},
  month={Aug},}@INPROCEEDINGS{9861000,
  author={Minna, Francesco and Massacci, Fabio and Tuma, Katja},
  booktitle={2022 IEEE 15th International Conference on Cloud Computing (CLOUD)}, 
  title={Towards a Security Stress-Test for Cloud Configurations}, 
  year={2022},
  volume={},
  number={},
  pages={191-196},
  abstract={Securing cloud configurations is an elusive task, which is left up to system administrators who have to base their decisions on "trial and error" experimentations or by observing good practices (e.g., CIS Benchmarks). We propose a knowledge, AND/OR, graphs approach to model cloud deployment security objects and vulnerabilities. In this way, we can capture relationships between configurations, permissions (e.g., CAP_SYS_ADMIN), and security profiles (e.g., AppArmor and SecComp). Such an approach allows us to suggest alternative and safer configurations, support administrators in the study of what-if scenarios, and scale the analysis to large scale deployments. We present an initial validation and illustrate the approach with three real vulnerabilities from known sources.},
  keywords={Cloud computing;Scalability;Heuristic algorithms;Containers;Benchmark testing;Security;Risk analysis;knowledge graph;AND/OR graphs;containers;security;microservices;cloud},
  doi={10.1109/CLOUD55607.2022.00038},
  ISSN={2159-6190},
  month={July},}@INPROCEEDINGS{9860731,
  author={Agarwal, Vikas and Butler, Chris and Degenaro, Lou and Kumar, Arun and Sailer, Anca and Steinder, Gosia},
  booktitle={2022 IEEE 15th International Conference on Cloud Computing (CLOUD)}, 
  title={Compliance-as-Code for Cybersecurity Automation in Hybrid Cloud}, 
  year={2022},
  volume={},
  number={},
  pages={427-437},
  abstract={Automation of cybersecurity processes has become crucial with large scale deployment of sensitive workloads in regulated on-prem, private, and public cloud environments. Regulatory and standards bodies such as Payment Card Industry (PCI), Federal Financial Institutions Examination Council (FFIEC), International Organization for Standardization (ISO), and others govern the minimal set of cybersecurity controls that an organization must implement. To meet such requirements while maintaining business agility, organizations need to modernize from manual document based compliance management to automated processes for continuous compliance. This modernized process is called compliance-as-code. In this paper, we present an architecture for compliance-as-code based on a standardized framework. We identify several design choices and our rationale behind those. Specifically, we introduce a system for manipulating compliance information in a standardized manner and a data interchange protocol for inter-operable communication of compliance information. We demonstrate the scalability of our approach and briefly describe deployment and experimental results in real world settings.},
  keywords={Industries;Automation;Protocols;Scalability;ISO Standards;ISO;Cloud computing security;security and compliance;compliance-as-code;continuous compliance;cloud security},
  doi={10.1109/CLOUD55607.2022.00066},
  ISSN={2159-6190},
  month={July},}@INPROCEEDINGS{9859402,
  author={Poursoltan, Milad and Pinède, Nathalie and Vallespir, Bruno and Traore, Mamadou Kaba},
  booktitle={2022 Annual Modeling and Simulation Conference (ANNSIM)}, 
  title={A New Modeling Framework For Cyber-Physical And Human Systems}, 
  year={2022},
  volume={},
  number={},
  pages={90-101},
  abstract={Health, manufacturing, and transport systems are in the midst of the rapid emergence of intelligent systems. In this regard, Cyber-Physical and Human Systems open a new window on intelligent systems in which the role of humans is prominent. Modeling and simulation (M&S) are recognized as practical tools playing a role in promoting the design, analysis, and development of CPHS. However, from a conceptual and technical point of view, CPHS modeling is challenging for modelers since they face some concepts and processes regarding human beings and intelligent artifacts which were not pervasive in M&S before. Thus, the aim of this study is twofold. The first is to shine new lights on the CPHS understanding regardless of application domains by providing an ontological model. The next is to propose an agent-based modeling framework according to the High-Level Language for Systems Specification (HiLLS) to convert conceptual models into executable ones.},
