  booktitle={2021 IEEE 6th International Conference on Cloud Computing and Big Data Analytics (ICCCBDA)}, 
  title={Construction and Application of Knowledge Graph for Commanders of Synthetic Brigade}, 
  year={2021},
  volume={},
  number={},
  pages={505-510},
  abstract={Synthetic brigade is the decisive force to carry out ground assault missions, Under the conditions of informationized warfare, battlefield information presents the characteristics of multi-source heterogeneity. How to transform information advantage into decision advantage is a big challenge for commanders. In this paper, the knowledge graph technology is used to design and implement the intelligent question and answer system for the commander of the synthetic brigade, which can effectively improve the combat effectiveness of the troops.},
  keywords={Cloud computing;Conferences;Force;Transforms;Big Data;commander of synthetic brigade;knowledge graph;QA system},
  doi={10.1109/ICCCBDA51879.2021.9442563},
  ISSN={},
  month={April},}@INPROCEEDINGS{9441481,
  author={Tahir, Alishba and Ijaz, Ahsan and Javed, Fahad},
  booktitle={2021 International Conference on Digital Futures and Transformative Technologies (ICoDT2)}, 
  title={Classy AA-NECTAR: Personalized Ubiquitous E-Learning Recommender System with Ontology and Data Science Techniques}, 
  year={2021},
  volume={},
  number={},
  pages={1-8},
  abstract={Learners have different learning styles each tailored to their own personality. Incompatibility of learning and teaching style is inconvenient. This paper integrates learner behavior modeling, academic web crawling and content retrieval using state of the art technology. This research work aims to propose a personalized ubiquitous learning model to identify learner learning styles and deploy type of content that is corresponding to the learner's learning style. Felder-Solomon model is one of the models being used for the learner profiling. This gives ease not only to the learners but the pedagogical instructors as well for not making different type of content. Real time monitoring makes the self-adaptive system learn through the learner's gestures and self-adjusts autonomously. Learners' aptitude increases, saving time and inconvenience. This will give an easy access to certifying organizations to get more capable skill oriented people.},
  keywords={Electronic learning;Education;Psychology;Ontologies;Licenses;Real-time systems;Data models;personalised e-learning;ontology;semantic web;educational data mining;learning style;adaptive learning;educational psychology;distance learning;cloud computing;ubiquitous learning;differentiated learning;individualized learning;recommended system},
  doi={10.1109/ICoDT252288.2021.9441481},
  ISSN={},
  month={May},}@INPROCEEDINGS{9407220,
