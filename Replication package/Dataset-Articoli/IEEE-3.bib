  title={Performance of Generating Dialogs from Ontology and Context}, 
  year={2018},
  volume={},
  number={},
  pages={1-6},
  abstract={In this paper we evaluate the performance of question generation using contextual reasoning in dialogs generated from an Ontology to provide identification, authentication, and access control. Generating relevant questions requires frequent querying after each question, using its context to generate the next one. This process is tedious and might lead to a situation where the interviewee has to wait for a question for an unacceptably long time. The objective of this work is to show that our method for generation of questions using context does not cause delays that might turn the dialogs useless.},
  keywords={Ontologies;Cognition;Benchmark testing;Delays;Access control;Speech recognition;Interviews;Performance Analysis;Semantic Web;Ontology;reasoning;Contextual Reasoning;Dialogs;IVR;Artificial Intelligence;Access Control;Security},
  doi={10.1109/AICCSA.2018.8612883},
  ISSN={2161-5330},
  month={Oct},}@INPROCEEDINGS{8599581,
  author={Wurster, Michael and Breitenbücher, Uwe and Képes, Kálmán and Leymann, Frank and Yussupov, Vladimir},
