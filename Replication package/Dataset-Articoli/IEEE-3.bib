  booktitle={2018 International Conference on Control, Automation and Diagnosis (ICCAD)}, 
  title={Implementation Approach for IDS based on Risk Assessment and Attack Pattern in Cloud Computing}, 
  year={2018},
  volume={},
  number={},
  pages={1-5},
  abstract={Cloud computing is an evolution in the computing area that takes advantage of the falling cost of CPU's and storage, combined with the ubiquity of broadband Internet access and dramatic increase of Internet connected devices. The impact of Cloud Computing technologies adoption include new standards, architecture paradigms, security challenges and disruptive business models. Security is a top concern in choosing the right cloud service provider. Analyzing intrusion based on attack patterns and risk assessment in cloud computing has demonstrated its efficiency in reducing the number of false alarms and optimizing the IDS performances. This paper intended to present the implementation of a new architecture to quantify attacks in cloud environment based on risk assessment. With the main objective to increase the efficiency of IDS and decrease the number of alarms. Experimental results show that our approach is better to the state-of-the-art approach for intrusion detection in cloud.},
  keywords={Cloud computing;Intrusion detection;Risk management;Computer architecture;Taxonomy;Ontologies;Cloud computing;IDS;IPS;RAPA;Behavioural analysis;risk assessment;taxonomy;attack pattern},
  doi={10.1109/CADIAG.2018.8751361},
  ISSN={},
  month={March},}@INPROCEEDINGS{8750721,
  author={Erascu, Madalina and Iuhasz, Gabriel and Micota, Flavia},
  booktitle={2018 20th International Symposium on Symbolic and Numeric Algorithms for Scientific Computing (SYNASC)}, 
  title={An Architecture for a Management Agency for Cloud Resources}, 
  year={2018},
  volume={},
  number={},
  pages={288-295},
  abstract={Cloud computing offers attractive options to migrate corporate applications without the end users needing to manage any physical resources. While this "ease" is appealing, several issues arise: 1) Which Cloud Providers (CPs) offer the best infrastructure at a fair budget? 2) I am no Cloud expert, then what are the characteristics of the infrastructure which best fit my application? To answer these questions, one must solve a resource management problem, that is, the allocation of computing, storage, networking and (indirectly) cost resources to a set of applications such that the performance objectives of the application are fulfilled. There are many approaches which answer separately these questions but there is no comprehensive and easily usable solution for these issues. MANeUveR solves them by integrating the following components: 1) a Web User Interface offers the end user the possibility to describe his application in terms of interactions between components and their software and hardware requirements, 2) an Offers Management System, through a crawler, periodically updates an ontology with infrastructure and services details from different CPs, 3) a Recommendation Engine provides a (sub) optimal solution for application deployment in the CP infrastructure regarding the leasing price of the virtual machines needed for deployment and their characteristics. Using a secure-billing e-mail service, we demonstrate the effectiveness of our solution.},
  keywords={Cloud computing;Ontologies;Crawlers;Tools;Software;Hardware;Genetic algorithms;Cloud Computing, crawling, ontology, optimization, SMT solving, metaheuristics},
  doi={10.1109/SYNASC.2018.00052},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{8672580,
  author={},
  booktitle={2018 International Arab Conference on Information Technology (ACIT)}, 
  title={ACIT 2018}, 
  year={2018},
  volume={},
  number={},
  pages={1-1},
  abstract={The following topics are dealt with: ontologies (artificial intelligence); natural language processing; cloud computing; pattern classification; control engineering computing; Internet; text analysis; business data processing; medical signal processing; organisational aspects.},
  keywords={Information technology;Conferences},
  doi={10.1109/ACIT.2018.8672580},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{8662953,
  author={Nagarajan, Rajganesh and Thirunavukarasu, Ramkumar},
