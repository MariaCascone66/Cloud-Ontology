  month={Oct},}@INPROCEEDINGS{8780992,
  author={Ye, Xinfeng},
  booktitle={2018 IEEE 4th International Conference on Computer and Communications (ICCC)}, 
  title={A Machine Learning Approach to Service Rule Conversion}, 
  year={2018},
  volume={},
  number={},
  pages={1961-1965},
  abstract={This paper proposed a scheme that automatically converts service rules of cloud manufacturing to semantic rules. The scheme classifies the semantics of typical service rules into several semantic categories. Sentence embedding is used to represent the semantics of service rules. A logistic regression classifier is used to identify the semantic category of a service rule according to its sentence embedding. Based on the identified semantic category, the service rule is converted to a semantic rule. The evaluation of the scheme shows that the scheme achieves good conversion accuracy.},
  keywords={Semantics;Companies;Manufacturing;Cloud computing;Drilling machines;Machine learning;Natural languages;semantic web;natural language processing;machine learning;cloud manufacturing},
  doi={10.1109/CompComm.2018.8780992},
