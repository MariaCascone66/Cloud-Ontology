  booktitle={2023 5th International Academic Exchange Conference on Science and Technology Innovation (IAECST)}, 
  title={Evaluation of Cloud Platform Operation and Maintenance Time Series Knowledge Graph State by Integrating Knowledge Graph and Graph Learning}, 
  year={2023},
  volume={},
  number={},
  pages={766-774},
  abstract={Cloud computing platforms have become an important infrastructure for modern enterprises to build business architectures, but with the continuous expansion of cloud platform scale, the difficulty of operation and maintenance has also increased exponentially. In order to better manage operation and maintenance data and improve operation and maintenance efficiency, this paper proposes a cloud platform operation and maintenance temporal knowledge graph state evaluation that integrates knowledge graph and graph learning. Based on the knowledge graph theory, a cloud platform operation and maintenance knowledge graph is constructed based on Sentence BERT, and the general static knowledge graph is extended to temporal knowledge graph, A graph convolutional neural network based reasoning method for cloud platform operation and maintenance temporal knowledge graph has been proposed. Through the above methods, the cloud platform operation and maintenance system can effectively capture the changes in temporal data and node feature information, improving the efficiency and quality of cloud platform operation and maintenance.},
  keywords={Cloud computing;Technological innovation;Systems operation;Time series analysis;Knowledge graphs;Voltage;Cognition;Knowledge graph;Cloud platform operation and maintenance;Graph learning;Graph Convolutional Neural Network},
  doi={10.1109/IAECST60924.2023.10503357},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{10494277,
  author={Wu, Qinglin and Wang, Yan},
  booktitle={2023 16th International Symposium on Computational Intelligence and Design (ISCID)}, 
  title={Research on Intelligent Question-Answering Systems Based on Large Language Models and Knowledge Graphs}, 
  year={2023},
  volume={},
  number={},
  pages={161-164},
  abstract={With the continuous development of artificial intelligence and cloud computing technologies, the emergence of large language models (LLMs) has created new opportunities for intelligent applications. However, large language models may lack authenticity and accuracy when providing answers in specific professional domains, and they even generate "illusory facts." In response to the limitations of current large language models in solving specific professional fields, this paper proposes to use large language models and knowledge graph technology to construct an intelligent question answering system for specific fields. Through systematic training and optimization, efficient domain specific knowledge Q&A has been achieved, improving the satisfaction rate of domain specific knowledge Q&A. The intelligent question answering system based on large models and knowledge graphs brings more convenience to people's lives and work, which is beneficial for users to obtain intelligent solutions in fields such as education, healthcare, and customer service.},
  keywords={Training;Computers;Systematics;Customer services;Computational modeling;Knowledge graphs;Medical services;large language models;knowledge graph;question-answer system;deep learning;natural language processing},
  doi={10.1109/ISCID59865.2023.00045},
  ISSN={2473-3547},
  month={Dec},}@INPROCEEDINGS{10487274,
  author={Wright, Steven A. and Sathyagiri, Achyuth and Tayal, Ravish},
  booktitle={2023 Congress in Computer Science, Computer Engineering, & Applied Computing (CSCE)}, 
  title={Machine Learning and the Secure Access Service Edge}, 
  year={2023},
  volume={},
  number={},
  pages={2251-2258},
  abstract={The Secure Access Service Edge (SASE) framework integrates network and security functions in a cloud native architecture. The paper reviews the SASE framework to examine the applicability and necessity for the inclusion of Machine learning (ML). The feasibility of ML at the edge, and its measurement and performance aspects, are reviewed in considering the feasibility of ML functions in the SASE framework. A proof-of-concept implementation is discussed to provide concrete examples of ML based services related to the service edge including monitoring, predictive alerting, and service disruption avoidance. The applicability and necessity are ultimately driven by the enterprise's objectives for its infrastructure. Enterprises adoption of ML services in a SASE framework may be driven by considering whether the radical network decentralization of recent years is a transient event.},
  keywords={Semantic Web;Industries;Cloud computing;Reviews;Machine learning;Security;Virtualization;Machine Learning (ML);Secure Access Service Edge (SASE);Communication Service Providers (CSPs)},
  doi={10.1109/CSCE60160.2023.00367},
  ISSN={},
  month={July},}@INPROCEEDINGS{10479231,
  author={Abidi, Sarra and Toumi, Samir and Essafi, Mehrez and Guegan, Chirine Ghedira and Ben Ghezala, Henda Hajjami},
  booktitle={2023 20th ACS/IEEE International Conference on Computer Systems and Applications (AICCSA)}, 
  title={Using a Correlation Equation to ensure Stability between Personalization and Security in Composing Web Services}, 
  year={2023},
  volume={},
  number={},
  pages={1-6},
  abstract={Because of the increasing complexity, openness and extensibility of services-based Information Systems (IS), Web Services Composition (WSC) became a main challenge that interests many researchers. This interest is growing with the apparition of Cloud applications that are being increasingly available and diversified trying to cover a multitude of services to satisfy the unlimited end-user requirements. In front of applications diversity, it become more difficult for end-user to find the most adequate service that perfectly meet their needs especially when considering personalization in WSC process. This difficulty increases when dealing with security constraints, especially in cloud environment where services are openly accessible and widely exposed to highly potential risks. In such context, WSC must be made in a careful way and personalization should proactively take into account protection measures against all eventual threats. Otherwise, integrating both personalization and security in WSC may result in an instable system.Based on this conviction, finding the correlation between these concepts remains an important issue. Hence, the key challenge of this paper is to study the stability system’s through a correlation equation between the precited concepts. To highlight the efficiency of our proposed equation, we will present an E-Learning application that was used during the COVID-19 pandemic.},
  keywords={COVID-19;Correlation;Electronic learning;Web services;Pandemics;Extensibility;Complexity theory;Web-Services-Composition;personalization;security;stability;correlation-equation},
  doi={10.1109/AICCSA59173.2023.10479231},
  ISSN={2161-5330},
  month={Dec},}@INPROCEEDINGS{10476188,
  author={Kundu, Rohon and Gehrmann, Christian and Kihl, Maria},
  booktitle={2023 IEEE 29th International Conference on Parallel and Distributed Systems (ICPADS)}, 
  title={A Comprehensive Robustness Analysis of Storj DCS Under Coordinated DDoS Attack}, 
  year={2023},
  volume={},
  number={},
  pages={659-666},
  abstract={Decentralized Cloud Storage (DCS) is considered to be the future for sustainable data storage within Web 3.0, in which we will move from a single cloud service provider to creating an ecosystem where anybody could be a cloud storage provider. Currently, the cloud storage market is highly dominated by centralized players like Amazon S3, Google Cloud, Box, etc. Decentralized projects like Storj, Filecoin, and Sia have seen rising popularity with the advent of Web 3.0 applications. At the same time, any blockchain network is susceptible to large-scale DDoS attacks. This work focuses on the Storj DCS, where we aimed to analyze the robustness of the system under the influence of a coordinated DDoS attack which can be carried out by an adversary or a group of adversaries taking down a set of storage nodes. The novelty of our work lies in threefold: First, we use statistical methods to mathematically model the content distribution as well as the loss of a file or a segment from the system. Our model captures both the cases where we have homogeneous and non-homogeneous nodes. Secondly, we develop a cost-analytic approach to perform a robustness analysis of the Storj system and implement the proposed model in MATLAB. Finally, we calculate the cost of a DDoS attack that the adversary has to incur in order to be successful with the attack. Also, we propose a set of better parametric choices for erasure piece distribution under which the system has proved to be more robust than the parametric values implemented in Storj DCS.},
  keywords={Semantic Web;Cloud computing;Analytical models;Costs;Probability;Denial-of-service attack;Robustness;DDoS Attack;Storj;Robustness;Decentralized Cloud Storage;Data Loss;Security},
  doi={10.1109/ICPADS60453.2023.00102},
  ISSN={2690-5965},
  month={Dec},}@INPROCEEDINGS{10451139,
  author={Yang, Ming and Zhang, Kaiyang and Mu, Chao and Wang, Xin and Liu, Yuanlong and Zhang, Heng},
  booktitle={2023 China Automation Congress (CAC)}, 
  title={A Shortest Path Query Approach for Encrypted Graphs Based on Padding Dictionary Structure}, 
  year={2023},
  volume={},
  number={},
  pages={8204-8208},
  abstract={Hybrid intelligence has emerged as an innovative approach that synergistically combines the strengths of human intelligence and artificial intelligence (AI) to address complex problems. In this paradigm, knowledge graphs play a pivotal role by supplying vital background information and contextual understanding to hybrid intelligence systems. However, due to limitations in local storage capacity, managing large-scale graph data often requires outsourcing to cloud servers. Nevertheless, the use of untrusted cloud servers poses a risk of potential user data leaks. To address these concerns, this paper presents an encryption scheme specifically designed to protect graph data, enabling queries involving intermediate nodes in the shortest path between two nodes within the graph. The proposed scheme ensures the privacy and security of outsourced data by employing a novel method that utilizes padding dictionary structure encryption. This method ensures strict limitations on the cloud server's capabilities, restricting it solely to query operations, it effectively prevents any compromise of user data or unauthorized access to sensitive information, users can securely store and process their graph data on servers, significantly reducing the potential risk of data leakage and unauthorized breaches. To validate the feasibility and effectiveness of the proposed solution, practical experiments were conducted using real-world data.},
  keywords={Dictionaries;Human intelligence;Knowledge graphs;Encryption;Outsourcing;Servers;Artificial intelligence;knowledge graph;graph encryption;shortest path queries;homomorphic encryption},
  doi={10.1109/CAC59555.2023.10451139},
  ISSN={2688-0938},
  month={Nov},}@INPROCEEDINGS{10451141,
  author={Sikri, Alisha and N, Gobi and Doda, Devendra Kumar},
  booktitle={2023 International Conference on Power Energy, Environment & Intelligent Control (PEEIC)}, 
  title={Renewal of Cloud-Based Services in Smart Grids}, 
  year={2023},
  volume={},
  number={},
  pages={1295-1299},
  abstract={Scalable solutions are required to handle fluctuating workloads in modern distribution grids in order to manage the flood of data from many sources, execute dynamic algorithms as needed, and effectively operate sparse actuators. In order to meet these needs, cloud computing technologies have surfaced as a suitable answer, facilitating effective data sharing and interchange. With semantic information integration playing a crucial role, this is especially important for fully exploiting the advantages of cloud computing in enabling sophisticated analytics and mining for smart grids. In order to analyze grid data for power distribution, this essay proposes integrating the semantic web into a cloud-based research platform. The idea recommends using a service restoration technique to reenergize loads after an electrical failure. This presentation is implemented using FIWARE, a platform composed of modular, open-source building pieces designed for future internet services and applications. Moreover, the SARGON ontology-which was created especially for the energy domain-is used here. We test the integration of semantic data into the cloud-based service restoration application using the IEC61850 data model. This integration provides an access to the distribution grid's field devices. The platform's performance test, measured in terms of computing time and network latency, validates the viability of the suggested solution. The results of these experiments provide as a standard against which to measure the performance of smart energy platforms in the future, therefore advancing the field.},
  keywords={Performance evaluation;Cloud computing;Heuristic algorithms;Semantics;Web and internet services;Data models;Time measurement;Cloud Computing;Service Restoration;Smart Grid;SARGON Ontology;Fault Location Isolation and Service Restoration (FLISR) and Distribution Management Systems (DMS)},
  doi={10.1109/PEEIC59336.2023.10451141},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{10428615,
  author={Jolly, Aman and Jaglan, Gaurav and Pandey, Vikas and Shashikant and Sharma, Meenakshi and Shrivastava, Vineet},
  booktitle={2023 3rd International Conference on Advancement in Electronics & Communication Engineering (AECE)}, 
  title={Leveraging Infrastructure Knowledge Graph for Cloud}, 
  year={2023},
  volume={},
  number={},
  pages={130-134},
  abstract={Semantic Web and Cloud Computing are two prominent areas within the field of Information Technology that offer distinct solutions to various challenges. The Semantic Web focuses on representing and organizing data in a structured and meaningful way, enabling effective data integration, interoperability, and knowledge discovery. On the other hand, Cloud Computing offers scalable and on-demand access to computing resources, enabling cost-effective storage, processing, and delivery of data and applications. This research article explores the integration of Infrastructure Knowledge Graph in cloud environments. It covers key topics in knowledge graph and cloud technologies, application of semantic web technologies, requirements, performance metrics, and key features of deploying a cloud infrastructure knowledge graph. The paper emphasizes the potential benefits of this integration, including enhanced resource management, improved scalability and performance, streamlined provisioning, data integration, and compliance. By leveraging these technologies, organizations can optimize their cloud infrastructure, drive innovation, and meet the demands of modern computing environment.},
  keywords={Semantic Web;Cloud computing;Scalability;Data integration;Knowledge graphs;Organizations;Resource management;Semantic web;Knowledge graph;Ontology;Cloud infrastructure;Infrastructure as a service},
