  booktitle={2024 Second International Conference on Inventive Computing and Informatics (ICICI)}, 
  title={Exploring the Frontiers of Blockchain and Web Technologies for a Dynamic Digital Future}, 
  year={2024},
  volume={},
  number={},
  pages={732-742},
  abstract={The combination of blockchain and web technology leads to a new era of digital ecosystems, with disruptive potential across several industries. This research study investigates the deep link between blockchain and web technologies, focusing on major ideas such as Web 3.0, decentralization, and smart contracts. The study's extensive literature assessment indicates the enormous impact of this integration, which extends beyond traditional fields such as healthcare and finance. Smart contracts, in particular, have emerged as powerful instruments for optimizing digital contracts and increasing transaction efficiency. Furthermore, Web 3.0's decentralization and empowerment aspects play an important role in strengthening digital systems. The report also discusses issues with blockchain- web integration and makes solutions for a more inventive, transparent, and secure digital future.},
  keywords={Industries;Semantic Web;Scalability;Smart contracts;Ecosystems;Process control;Transforms;Blockchain;Web 3.0;Interoperability;Decentralization;Smart Contracts;Web Technology;Digital Future},
  doi={10.1109/ICICI62254.2024.00124},
  ISSN={},
  month={June},}@INPROCEEDINGS{10649818,
  author={Ming Jie, Christensen Choong and Rajah, Daveena Sri A-P Michael and Ganisen, Gyvayhni A-P and Chandran, Tharanitharan A-L and Rafsanjani, Ahmad Sahban and Basheer Jasser, Muhammed},
  booktitle={2024 IEEE International Conference on Automatic Control and Intelligent Systems (I2CACIS)}, 
  title={A Secure Data Sharing Platform in the Era of Web 3.0 by Leveraging the Power of Blockchain Technology}, 
  year={2024},
  volume={},
  number={},
  pages={291-296},
  abstract={In the ever-evolving landscape of the internet, the emergence of Web 3.0 has ushered in a new era of decentralized applications. As researchers navigate through Web 3.0, the criticality of secure data sharing becomes evident, necessitating a robust solution to address vulnerabilities present in current methods. The existing methods grapple with vulnerabilities, especially in centralized repositories susceptible to breaches due to intricate and less secure authentication processes. Furthermore, the compromise of data integrity and privacy during the process of data sharing exacerbates the challenges faced by current methods. This research conducts a thorough examination of the pivotal role played by blockchain in enhancing security measures and empowering consumers with control over their data. Furthermore, it introduces a novel approach by proposing a secure Web 3.0 data-sharing platform, leveraging IPFS, Pinata, Smart Contracts, Meta Masks, and decentralized principles. To evaluate the security of the proposed secure data sharing platform, this study utilized two smart contract vulnerability detector tools, namely Slither and Echidna. The findings revealed a minimal presence of issues, with Slither identifying only five low-level, nonthreatening problems and Echidna indicating no critical failures. This demonstrates the platform's robustness against significant vulnerabilities such as the reentrancy attack and integer overflow and validates its ability to facilitate safe, decentralized data sharing in the context of the Web 3.0 paradigm.},
  keywords={Semantic Web;Data privacy;Navigation;Current measurement;Data security;Data integrity;Smart contracts;IPFS;WEB 3.0;Data Sharing;Smart Contracts;Pinata;Blockchain},
  doi={10.1109/I2CACIS61270.2024.10649818},
  ISSN={2995-2859},
  month={June},}@INPROCEEDINGS{10633363,
  author={Sawada, Rempei and Takano, Haruto and Mori, Kosuke and Teraoka, Fumio},
  booktitle={2024 IEEE 48th Annual Computers, Software, and Applications Conference (COMPSAC)}, 
  title={MECM2M: An MEC Based M2M System Considering Mobile Nodes as Compute Servers}, 
  year={2024},
  volume={},
  number={},
  pages={765-775},
  abstract={In addition to stationary sensors/actuators (nodes), mobile nodes such as vehicles and smartphones are connected to M2M (Machine-to-Machine) systems. Some M2M systems take into account node mobility, while some M2M systems regard vehicles as compute servers. N one of them, however, handle the mobility of nodes that act as compute servers. This paper proposes an MEC (Multi-access Edge Computing) based M2M system called MECM2M, which can handle the mobility of nodes acting as sensors/actuators and compute servers. The design policy of MECM2M is analogous to that of the Internet. The Internet provides two fundamental services: reliable byte stream and unreliable datagram. How data is used is up to applications. MECM2M provides M2M applications with three fundamental services as the M2M services: area-based data acquisition, node-based data acquisition, and node-based action instruction. How sensing data is used is up to M2M applications. MECM2M introduces the virtual nodes as digital twins in the virtual space for the physical nodes. The virtual nodes conceal differences from the physical nodes and provide a uniform interface. The relation among the virtual nodes, the physical nodes, and the locations of the physical nodes is managed by a domain ontology called Onto- Mecm2m,which enables effective processing of the three fundamental services. The evaluation results on an MECM2M emulator show that the response times of the three fundamental services satisfy the latency constraints, i.e., 100 ms, defined in an ETSI (European Telecommunications Standards Institute) document.},
  keywords={Machine-to-machine communications;Cloud computing;Data acquisition;ETSI;Computer architecture;Containers;Software;M2M;loT;MEC;mobility support;digital twins},
  doi={10.1109/COMPSAC61105.2024.00108},
  ISSN={2836-3795},
  month={July},}@INPROCEEDINGS{10633417,
  author={Farhad, Mohammad and Saha, Gourab and Nahid, Masum Alam and Chowdhury, Fairuz Rahaman and Paul, Partha Protim and Ullah, Mohammed Raihan and Ferdous, Md Sadek},
  booktitle={2024 IEEE 48th Annual Computers, Software, and Applications Conference (COMPSAC)}, 
  title={Secure Backup and Recovery of SSI Wallets using Solid Pod Technology}, 
  year={2024},
  volume={},
  number={},
  pages={1101-1111},
  abstract={A new paradigm for digital identity management called Self-Sovereign Identity (SSI) has emerged to offer users more control over their identity data. An important component of SSI is a wallet that stores cryptographic keys and other identity data. Secure backup and recovery methods for data stored in such wallets are a crucial feature for its wide-scale adoption, however, such a feature is lacking or implemented casually in the existing SSI wallets. In this research, we propose a secure backup and storage of SSI wallets using a novel technology called Solid Pod. Solid Pod is an emerging technology that enables users to securely store data online. Towards this aim, we present the architecture, based on a threat model and requirement analysis, of the proposed approach. We also discuss its implementation details, outline a number of protocol flows highlighting different use-cases and analyse its security, advantages and limitations.},
  keywords={Threat modeling;Protocols;Computer architecture;Solids;Software;Cryptography;Secure storage;Self-Sovereign Identity (SSI);SSI Wallet;Social Linked Data;Solid Pod;Blockchain;Hyperledger Aries;Hyper-ledger Indy},
  doi={10.1109/COMPSAC61105.2024.00149},
  ISSN={2836-3795},
  month={July},}@INPROCEEDINGS{10605150,
  author={Yujia, Liu and Tao, Xu and Zhehan, Su and Xinsong, Li and Kaipeng, Xue and Yuxin, Liu},
  booktitle={2024 IEEE 11th International Conference on Cyber Security and Cloud Computing (CSCloud)}, 
  title={Entity Alignment Through Joint Utilization of Multiple Pretrained Models for Attribution Relationship}, 
  year={2024},
  volume={},
  number={},
  pages={1-6},
  abstract={Most existing models for entity alignment tasks are based on a single pre-training mode. Through multiple experiments, it has been verified that a single pre-training model is no longer sufficient to meet the needs of current entity alignment tasks. Based on considerations of multiple pre-training models, this paper proposes an entity alignment method that utilizes multiple pre-training models. The purpose of this method is to address the limited accuracy in entity alignment tasks, with a particular focus on separately pre-training the embedding of entity attribute information and link relationship information for matching. To achieve the goal of improving entity alignment accuracy, this paper adopts joint alignment using pre-training with BERT and GloVe word embeddings. Experimental evaluations are conducted on the DBP15k dataset, and the results demonstrate that leveraging multiple pre-training models for attribute and relationship joint alignment can enhance model performance. The evaluation metrics of the model on three different language datasets, Hits@1 and MRR, are as follows: 0.899, 0.930, 0.964, and 0.932, 0.951, 0.975, respectively. These metrics indicate that the model performs well in aligning entities and outperforms current mainstream entity alignment methods.},
  keywords={Training;Measurement;Deep learning;Cloud computing;Accuracy;Computational modeling;Semantics;knowledge graph;entity alignment;BERT;GloVe;joint alignment},
  doi={10.1109/CSCloud62866.2024.00008},
  ISSN={2693-8928},
  month={June},}@INPROCEEDINGS{10593024,
  author={D, Preethi and Mehta, Poornima and Sood, Devendra},
  booktitle={2024 International Conference on Communication, Computer Sciences and Engineering (IC3SE)}, 
  title={Automated Semantic Interpretations in Web Applications via Cloud Computing Aspects}, 
  year={2024},
  volume={},
  number={},
  pages={1-5},
  abstract={The creation and execution of applications in several disciplines have been greatly aided by the integration of cloud computing and semantic web technologies. Although they are independent entities, they may also be combined in many ways to provide answers—a subject that has been thoroughly researched lately. The number of “new cloud providers, applications, facilities, management systems, and data” has significantly increased in recent years. The complexity that has resulted from this expansion emphasises the need for new technology in order to efficiently manage and support these varied and many services and resources. As a result, difficulties with “cloud services and resources” transferability, compatibility, security, choice, negotiation, identity, and specification might arise. Semantic Technologies are a vital tool for reassessing these issues because of their enormous “cloud computing” capacity. This study uses a variety of sources to examine and evaluate the impact of “Semantic-Web Technology” in the cloud. Moreover, a cloud-based engagement strategy shows how we may leverage the features and advantages of cloud computing to build the semantic web and provide automatic semantic annotations to online applications on a large scale.},
  keywords={Semantic Web;Cloud computing;Annotations;Complexity theory;Security;Facilities management;Semantic technology;cloud-driven;semantic technologies;cloud computing;semantic web},
  doi={10.1109/IC3SE62002.2024.10593024},
  ISSN={},
  month={May},}@INPROCEEDINGS{10598090,
  author={Li, Siyu and Zhang, Zhiwei and Xiao, Jiang and Zhang, Meihui and Yuan, Ye and Wang, Guoren},
  booktitle={2024 IEEE 40th International Conference on Data Engineering (ICDE)}, 
  title={Authenticated Keyword Search on Large-Scale Graphs in Hybrid-Storage Blockchains}, 
  year={2024},
  volume={},
  number={},
  pages={1958-1971},
  abstract={The widespread availability of Internet access and online services has led to the generation of numerous large-scale graphs in various real-world applications, such as online social networks and knowledge graphs. Keyword search stands out as a crucial task in the analysis and mining of these graphs. However, graph data owners tend to outsource storage and computation tasks to the cloud due to limited computing and storage resources. In this case, it is critical to ensure the integrity of the query results, as the cloud may have an incentive to return tampered results to serve its own interests. Currently, blockchain systems can store data efficiently and securely, creating a decentralized, tamper-proof digital platform. This functionality positions blockchain as a crucial complement and enhancement to traditional cloud storage solutions. Mainstream blockchains use a hybrid storage system to improve scalability, storing small meta-data on-chain and outsourcing raw data off-chain. While cryptographic proofs protect data integrity for queries, current schemes only support key-value data. This paper pioneers the study of authenticated keyword searches on graphs in hybrid-storage blockchains. The key challenge is to design an authenticated data structure (ADS) based on the graph data that can efficiently deal with keyword search queries. We propose Merkle Path DAG (MP-DAG), a novel ADS that aggregates the unqualified paths that will not appear in the result trees to efficiently handle authenticated keyword search queries on graphs. Furthermore, to reduce the ADS storage cost, we design an optimization scheme MP-DAG* by combining the similar subgraphs of MP-DAG. Experimental results demonstrate the performance of the proposed ADS and optimization measure.},
  keywords={Cloud computing;Costs;Social networking (online);Scalability;Keyword search;Knowledge graphs;Blockchains;n/a},
  doi={10.1109/ICDE60146.2024.00155},
  ISSN={2375-026X},
  month={May},}@INPROCEEDINGS{10592472,
  author={Zhao, Huiying and Li, Hongwu and Wu, Bin and Liu, Ruiqi and Xu, Lexi and Huang, Bingming and Xie, Zhipu and Cheng, Xinzhou},
  booktitle={2024 International Wireless Communications and Mobile Computing (IWCMC)}, 
  title={Cross-Layer Alarm Association Rules Discovery of Cloud-Network based on Knowledge Graph}, 
  year={2024},
  volume={},
  number={},
  pages={400-405},
  abstract={The fragmented architecture, cloud-based infrastructure, and functionally virtualized network elements within the 5 G core network have significantly surged the volume and diversity of alarms generated on cloud network service platforms that it supports. Given the inherently cross-layered nature of failure scenarios on these platforms, identifying the root causes presents a significant challenge. Alarm association rule mining has become an effective means to address the problems of alarm correlation and root cause localization. In this paper, an explainable alarm association rule mining approach based on knowledge graph, referred to as ARK-G, is proposed. Initially, a cloud-network cross-layer alarm association knowledge graph (CA2KG) is constructed. Subsequently, the knowledge embedding based graph convolutional network is employed to perform knowledge graph embedding on CA2KG. This embedding is then utilized to enhance the RNNLogic algorithm, thereby facilitating cross-layer alarm association rule mining with interpretable paths. Finally, a weighted rule tree is derived from a subset of CA2KG and the generated explainable rules, enabling the deduction of the root alarm. Experimental results demonstrate that the proposed ARK-G approach for association rule mining yields a higher hit rate compared to the baseline model, which provides valuable assistance in the faults analysis of 5 G cloud-network platforms.},
  keywords={Location awareness;Cloud computing;Cross layer design;Reviews;Wireless networks;Neural networks;Knowledge graphs;network intelligence;graph neural networks;knowledge graph;alarm association rule;root cause analysis},
  doi={10.1109/IWCMC61514.2024.10592472},
  ISSN={2376-6506},
  month={May},}@INPROCEEDINGS{10580547,
  author={Li, Pengsheng and Du, Qingfeng and Zhao, Shengjie and Fang, Pei},
  booktitle={2024 27th International Conference on Computer Supported Cooperative Work in Design (CSCWD)}, 
  title={Advancing Root Cause Analysis in Cloud-native System with Knowledge Graph Path Embedding Translation}, 
  year={2024},
  volume={},
  number={},
  pages={3134-3139},
  abstract={Cloud computing technologies, including cloud-native and containerization, have gained prominence in recent years, attributed to their exceptional scalability, enhanced resource utilization, and expedited deployment capabilities. However, their inherent complexity and the intricate interplay of internal components heighten the risk of sporadic and unforeseen anomalies. To address these challenges, Root Cause Analysis (RCA) is employed to accurately identify problematic services (pods) and mine the precise faults behind observed anomalies. Tailored to the limitations of conventional RCA algorithms, we propose a novel approach that jointly models operation entities and their relationships as learnable embeddings. Additionally, this method integrates fault propagation information to further improve RCA accuracy. Our evaluation involves developing a prototype within the Kubernetes cloud-native system. Extensive experimental results validate the efficacy of our approach.},
  keywords={Fault diagnosis;Computers;Cloud computing;Root cause analysis;Federated learning;Scalability;Prototypes;cloud native;root cause analysis;anomaly path;knowledge graph},
  doi={10.1109/CSCWD61410.2024.10580547},
  ISSN={2768-1904},
  month={May},}@INPROCEEDINGS{10569625,
  author={Madani, Yolo and Akanbi, Adeyinka},
  booktitle={2024 IST-Africa Conference (IST-Africa)}, 
  title={A Semantic Middleware using Docker and Kubernetes Orchestration Tools}, 
  year={2024},
  volume={},
  number={},
  pages={01-12},
  abstract={The use of application containerization in the form of technologies such as Docker and Kubernetes, brings new levels of efficiency, scalability, and portability to the design and implementation of semantic middleware. Containerization technologies such as Docker and Kubernetes offer standardized and efficient approaches for packaging, deploying, and managing applications in distributed environments. The need to bridge the gap between different heterogeneous systems and to ensure meaningful communication between the systems is where the semantic representation mechanism plays an important role. This mechanism is essential to a shared language and meaning, enabling effective communication and collaboration. Using Docker for containerization we can encapsulate these semantically enriched heterogeneous systems in isolated environments, ensuring their seamless deployment and management. This ensures that sensor reading and user notifications are effectively exchanged between different components in the system. As a container orchestration platform, Kubernetes enables autoscaling, load balancing, and fault tolerance, and ensures high availability and reliability of Multi-Hazard Early Warning System (MHEWS), even if the infrastructure fails. This enables easy integration and interoperability of various software components and increases system scalability and reliability. In this study, we detail architectural design and implementation considerations for the Multi-Hazard Early Warning System, with a focus on using semantic middleware and containerization technologies, we will explore how the technologies can improve data interoperability and improve system performance.},
  keywords={Scalability;Semantics;Smart contracts;Alarm systems;Solids;Blockchains;Software reliability;Semantic Middleware;Docker;Kubernetes;Containerization;Multi-Hazard Early Warning System},
  doi={10.23919/IST-Africa63983.2024.10569625},
  ISSN={2576-8581},
  month={May},}@INPROCEEDINGS{10554686,
  author={Srinivas, Pooja and Husain, Fiza and Parayil, Anjaly and Choure, Ayush and Bansal, Chetan and Rajmohan, Saravan},
  booktitle={2024 IEEE/ACM 46th International Conference on Software Engineering: Software Engineering in Practice (ICSE-SEIP)}, 
  title={Intelligent Monitoring Framework for Cloud Services: A Data-Driven Approach}, 
  year={2024},
  volume={},
  number={},
  pages={381-391},
  abstract={Cloud service owners need to continuously monitor their services to ensure high availability and reliability. Gaps in monitoring can lead to delay in incident detection and significant negative customer impact. Current process of monitor creation is ad-hoc and reactive in nature. Developers create monitors using their tribal knowledge and, primarily, a trial and error based process. As a result, monitors often have incomplete coverage which leads to production issues, or, redundancy which results in noise and wasted effort. In this work, we address this issue by proposing an intelli-gent monitoring framework that recommends monitors for cloud services based on their service properties. We start by mining the attributes of 30,000+ monitors from 791 production services at Microsoft and derive a structured ontology for monitors. We focus on two crucial dimensions: what to monitor (resources) and which metrics to monitor. We conduct an extensive empirical study and derive key insights on the major classes of monitors employed by cloud services at Microsoft, their associated dimensions, and the interrelationship between service properties and this ontology. Using these insights, we propose a deep learning based framework that recommends monitors based on the service properties. Finally, we conduct a user study with engineers from Microsoft which demonstrates the usefulness of the proposed framework. The proposed framework along with the ontology driven projections, succeeded in creating production quality rec-ommendations for majority of resource classes. This was also validated by the users from the study who rated the framework's usefulness as 4.27 out of 5.},
  keywords={Measurement;Deep learning;Noise;Termination of employment;Production;Ontologies;Delays;Cloud Services;Reliability;Intelligent Monitoring},
