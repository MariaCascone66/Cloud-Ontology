  month={Nov},}@INPROCEEDINGS{10825897,
  author={Pan, Yiting},
  booktitle={2024 IEEE International Conference on Big Data (BigData)}, 
  title={A New Perspective for the Study of "Five Thousand Years of Chinese History ": Visual Analysis of Single Text and Time Series Text}, 
  year={2024},
  volume={},
  number={},
  pages={8793-8797},
  abstract={Digital humanities introduces knowledge graph, visual analysis and other technologies into humanities research, which provides a new method for the inheritance and dissemination of traditional Chinese culture. This paper visually analyzes the text of Five Thousand Years of Chinese history from three dimensions: keyword word cloud map, character relationship knowledge graph, and Sankey map of historical events. Firstly, the keyword cloud map was realized by Python and multiple third-party libraries, which was convenient for readers to quickly appreciate the key points expressed in the text. Secondly, entity recognition was performed through hanLP, triple relationships were obtained through the deep learning model, and the knowledge graph of character relationships was stored through Neo4j, which was convenient for readers to understand historical characters and clarify the relationship between characters. Finally, Sankey diagrams are generated according to the characters, events and time order through the ECharts visualization chart library, which is convenient for readers to sort out the relationship between characters and events, and clarify the historical order. It provides a new perspective for the visual analysis of single text and temporal text in the field of digital humanities research, and provides a broader foundation for future research.},
  keywords={Deep learning;Visualization;Cloud computing;Time series analysis;Mixed reality;Knowledge graphs;Tag clouds;Libraries;History;Character recognition;digital humanities;knowledge graph;visual analysis;time series text;sankey diagram},
  doi={10.1109/BigData62323.2024.10825897},
