  ISSN={},
  month={Oct},}@INPROCEEDINGS{10822796,
  author={Cecil, Roy R. and Miranda, João L. and Nagy, Mariana and Da Silveira, Marcos and Zanin, Massimiliano},
  booktitle={2024 IEEE International Conference on Bioinformatics and Biomedicine (BIBM)}, 
  title={Advanced Methodologies and Technologies for Innovative Information Systems}, 
  year={2024},
  volume={},
  number={},
  pages={6745-6752},
  abstract={Nowadays, Information Systems (IS) in healthcare systems are being successively updated, either incorporating advanced Optimization tools combined with Artificial Intelligence (AI) and Large Language Models (LLM), or using innovative computing features for storage, processing, security, privacy, and friendly interface. Large efforts are required to design, validate and implement such developments, diverse communities in multiple scientific domains are involved, and new approaches are key to cope with the transdisciplinary gaps. In this paper we outline and discuss the relevant technologies and methodologies for IS within the context of health emergencies. Advanced and innovative approaches are being applied within the Health Informatics area, and we address a set of key technologies (e.g., Cloud Computing, AI-based platforms, Neural Networks) and methodologies (e.g., Knowledge Graphs, advanced Optimization, Multi-Criteria Decision Making) that are being relevant within the Health Emergency Responses (HER) framework, and also for the health informatics communities.},
  keywords={Privacy;Large language models;Neural networks;Medical services;MCDM;Security;Bioinformatics;Optimization;Secure storage;Information systems;Health Emergency Responses (HER);Methodologies;Technologies;Data Analytics;Knowledge Graphs;Intelligent Platforms},
  doi={10.1109/BIBM62325.2024.10822796},
  ISSN={2156-1133},
  month={Dec},}@INPROCEEDINGS{10803353,
  author={Moreschini, Sergio and Albano, Michele and Hastbacka, David},
  booktitle={2024 50th Euromicro Conference on Software Engineering and Advanced Applications (SEAA)}, 
  title={Best Practices for Resource Provisioning Declaration Within the Cognitive Cloud Continuum}, 
  year={2024},
  volume={},
  number={},
  pages={134-137},
  abstract={The evolution of cloud computing, driven by ad-vances in mobile, edge technologies, and AI, has led to the development of the Cognitive Cloud Continuum (COCLCON). However, this paradigm introduces new challenges in managing and optimizing computing resources across a heterogeneous environment. This paper explores best practices for declaring resources within COCLCON, with a focus on efficient resource allocation and transparently declaring available resources by de-vices. In this study, we undertook a non-holistic literature review to identify current technologies used to specify requirements and to determine current gaps in best practices. The main outcome of our work is a proposed schema for Resource Provisioning Declaration, which will allow for increased knowledge related to the available devices and resources within the COCLCON.},
  keywords={Cloud computing;Bibliographies;Computer architecture;Resource management;Artificial intelligence;Best practices;Software engineering;Kubernetes;Ontology;Serverless;Resources},
  doi={10.1109/SEAA64295.2024.00028},
  ISSN={2376-9521},
  month={Aug},}@INPROCEEDINGS{10805737,
  author={Nadeem, Mubaris and Bender, Lisa and Weber, Christian and Hahn, Kai},
  booktitle={2024 E-Health and Bioengineering Conference (EHB)}, 
  title={Knowledge Fusion Through Ambient Smart Living Technology and Telemonitoring in Emergency Response}, 
  year={2024},
  volume={},
  number={},
  pages={1-4},
  abstract={With the help of knowledge fusion methods, it is possible to interconnect different sources of knowledge, enhancing the individual knowledge fields, enabling to connect various, compatible kinds of systems for the purposes of decision-making or tailored recommendations. Especially in the field of healthcare and medicine, such advancements can support health professionals in providing patient-driven, fast and accurate treatments. For this, a common ground is needed where data can be stored and accessed in real time. This, in addition to medical digital services, will help facilitating a timely analysis of specific patients. Particularly, the concept of a digital twin (DT) is suitable to help and is profiting from a knowledge fusion perspective. This paper focuses on the idea of merging ambient smart living technologies with health parameters (such as vital signs) to develop a DT -concept, integrated into rescue operations as a DT -service. An alarm event detected by this DT, such as a heart attack, can lead then to an early rescue operation, resulting in a fast treatment of the patient. In this paper, a concept for the use of a DT and DT services, combined with the use of knowledge fusion will be elaborated in detail.},
  keywords={Cloud computing;Medical devices;Merging;Decision making;Medical services;Machine learning;Cardiac arrest;Emergency services;Real-time systems;Digital twins;ambient smart living;rescue operation;digital twin;knowledge graph;telemonitoring},
  doi={10.1109/EHB64556.2024.10805737},
  ISSN={2575-5145},
  month={Nov},}@INPROCEEDINGS{10788148,
  author={Qinyi, Li},
  booktitle={2024 International Conference on Computers, Information Processing and Advanced Education (CIPAE)}, 
  title={Research on Data-Driven Financial Risk Prediction Model Based on Cloud Computing}, 
  year={2024},
  volume={},
  number={},
  pages={778-782},
  abstract={This article aims to explore a data-driven financial risk prediction model based on cloud computing technology. This model achieves accurate identification and prediction of corporate financial risks by integrating advanced algorithms and knowledge graph technology. This article proposes a new risk prediction algorithm model that combines multiple methods such as machine learning, deep learning, and statistical analysis to learn patterns from historical data and predict future financial risks. In addition, the application of knowledge graph enables the model to better understand the internal connections of financial data and improve the accuracy of predictions. Finally, through system simulation experiments, this paper verifies the effectiveness and stability of the proposed model in practical applications. Experimental results show that this model can significantly improve the accuracy of financial risk prediction and provide powerful decision support for enterprises. This research not only enriches the theoretical system in the field of financial risk management, but also provides new ideas and technical means for the practice of financial risk management and control of enterprises.},
  keywords={Cloud computing;Analytical models;Accuracy;Computational modeling;Knowledge graphs;Predictive models;Prediction algorithms;Data models;Systems simulation;Risk management;Cloud computing;data-driven;financial risk prediction;risk management and control system;knowledge graph;system simulation},
  doi={10.1109/CIPAE64326.2024.00147},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{10793303,
  author={Chen, Zhiqiang and Zhao, Yue and Teng, Fei and Hu, Jie and Tian, Bo and Zhang, Hao},
  booktitle={2024 16th International Conference on Communication Software and Networks (ICCSN)}, 
  title={Dynamic Searchable Symmetric Encrypted Knowledge Graph}, 
  year={2024},
  volume={},
  number={},
  pages={20-27},
  abstract={With the rapid development of knowledge graphs and cloud computing, most people choose to store knowledge graphs on commercial cloud platforms. However, since cloud platforms are not completely trustworthy, this exposes graph data to security risks. Therefore, protecting knowledge graph data stored on third-party cloud platforms is essential. Based on these considerations, we propose a Dynamic Searchable Symmetric Encryption-based Knowledge Graph scheme (DSSE-KG). DSSE-KG aims to address the privacy and security risks associated with storing knowledge graph data on cloud servers. Compared to existing schemes, DSSE-KG not only ensures the data security of knowledge graphs but also provides multi-hop relationship retrieval and dynamic update capabilities. Performance tests on multiple publicly available real-world datasets demonstrate that the DSSE-KG scheme achieves a good balance between security, functionality, and efficiency for knowledge graphs.},
  keywords={Knowledge engineering;Cloud computing;Data privacy;Knowledge graphs;Software;Real-time systems;Performance analysis;Cryptography;Servers;Indexing;knowledge graph;data security;dynamic searchable encryption;multi-hop retrieval;dynamic update},
  doi={10.1109/ICCSN63464.2024.10793303},
  ISSN={2472-8489},
  month={Oct},}@INPROCEEDINGS{10778878,
  author={Yuan, Meng and Wu, Huaiyu and Shen, Zhen and Han, Yunjun and Zhou, Jiehan and Yue, Zhijian and Xiong, Gang},
  booktitle={2024 IEEE 4th International Conference on Digital Twins and Parallel Intelligence (DTPI)}, 
  title={Design and Implementation of a Web-based Collaborative Manufacturing System Based on Knowledge Graph}, 
  year={2024},
  volume={},
  number={},
  pages={524-528},
  abstract={Collaborative manufacturing is central to agile, intelligent, and cloud-based manufacturing paradigms. This paper presents the design and implementation of a web-based collaborative manufacturing system based on a knowledge graph (KG). The system achieves the full spectrum of knowledge extraction, fusion, and reasoning across the industrial chain, thereby enhancing efficiency and reducing operational costs. The proposed system constructs an industrial chain KG and multi-agent systems, offering comprehensive digital services across the manufacturing value chain. It supports the integration of business processes, data, and standard information for enterprise groups, presenting collaborative manufacturing data of the industrial chain in real-time visualization. The paper further discusses a method for real-time perception of anomalies in the collaborative manufacturing process, utilizing expert knowledge and data-driven insights to construct an operational risk KG. The system’s architecture, workflow, and functional design are detailed, along with application cases in visual dynamic management. The conclusion underscores the system’s potential to improve supply-demand chain management and operational efficiency in collaborative manufacturing.},
  keywords={Knowledge engineering;Visualization;Manufacturing processes;Collaboration;Data visualization;Knowledge graphs;Real-time systems;Digital twins;Standards;Multi-agent systems;knowledge graph;collaborative manufacturing;industry chain;supply chain;demand chain},
  doi={10.1109/DTPI61353.2024.10778878},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10777409,
  author={Liu, Yunhao and Qin, Xiaolin},
  booktitle={2024 4th International Conference on Intelligent Technology and Embedded Systems (ICITES)}, 
  title={A Reliable Artificial Intelligence System Architecture Based on Knowledge Reasoning}, 
  year={2024},
  volume={},
  number={},
  pages={195-200},
  abstract={Artificial intelligence (AI) technology is rapidly advancing and gradually sweeping into various domains, bringing profound changes to production and daily life. However, with the opportunities presented by AI technology come inherent risks. Ensuring AI is safe, reliable, and controllable to enhance users’ confidence is crucial for the long-term development of AI. Developing trustworthy AI involves focusing on aspects such as reliability, interpretability, fairness, robustness, and privacy protection. It is of paramount importance to constructing a reliable class of AI systems. Addressing the reliability issues prevalent in current widely deployed AI systems, this paper proposes an architecture for a reliable AI system based on knowledge reasoning. The aim is to achieve efficient, accurate and reliable knowledge processing and reasoning. AI-Math, a knowledge graph-based question-answering system developed according to this architecture, serves as an effective application example of reliable artificial intelligence.},
  keywords={Computer network reliability;Systems architecture;Computer architecture;Speech recognition;Chatbots;Cognition;Robustness;Reliability;Artificial intelligence;Protection;reliable artificial intelligence;trustworthy artificial intelligence;system architecture;knowledge reasoning},
  doi={10.1109/ICITES62688.2024.10777409},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{10758468,
  author={Gribova, V. and Shalfeeva, E.},
  booktitle={2024 IEEE International Multi-Conference on Engineering, Computer and Information Sciences (SIBIRCON)}, 
  title={Cloud Technology, Infrastructure and Computing for Manufacturing Viable XAI Systems}, 
  year={2024},
  volume={},
  number={},
  pages={288-293},
  abstract={The hot topic in the field of artificial intelligence is the production of intelligent products based on knowledge graphs as effective method for building applications of explainable Artificial Intelligence (XAI). The success of AI-products directly depends on how effective the viability mechanisms are laid down in its design. In this paper, a systematic cloud-based toolkit is proposed to develop viable XAI systems. The characteristic properties of applicated intelligent software services with knowledge graphs are described. It is explained, how these properties are provided in cloud specialized manufacturing environments. The example of tools for cognitologs and engineers to create portals with XAI-system production conditions are the instruments implemented on the «IACPaaS» platform. Several thematic cloud portals for collective development of XAI services have already been created here. An example is the specialized manufacturing environment of the Medical Portal, which made it possible to create multifunctional clinical software services to support decision-making based on diseases-centric knowledge graphs, the completeness and relevance of which is maintained remotely.},
  keywords={Knowledge engineering;Systematics;Explainable AI;Instruments;Knowledge graphs;Production;Software;Manufacturing;Portals;Diseases;knowledge graph;XAI-system;explanation generation;knowledge intelligent services;cloud-based toolkit;manufacturing environment},
  doi={10.1109/SIBIRCON63777.2024.10758468},
