@INPROCEEDINGS{11176626,
  author={Chowdhury, Mahfuzulhoq and Priya, Mst. Sanjida Jamal and Khatun, Mst. Tuly and Chowdhury, Rakib Uddin and Teya, Urshi Barua and Sultana, Suriya},
  booktitle={2025 8th International Conference on Circuit, Power & Computing Technologies (ICCPCT)}, 
  title={Synergy: A Ceramic, Construction, and Home Decor Industry Assistant Smart Mobile Application with Vendor Search, Verification, Item Purchase Features}, 
  year={2025},
  volume={},
  number={},
  pages={1609-1614},
  abstract={Ceramics, home furnishings, and building construction industries are essential to a nation’s economy because they create jobs, provide critical goods, and generate foreign exchange. An automated ceramic, home decor, and building construction business assistance system with vendor fraud detection, online vendor search, item purchase, vendor verification, contact, interior designer search, exhibition, advertisement, and quotation facilities were not developed in the existing literature. To mitigate previous limitations, this paper displays a flutter-based mobile application for ceramic, building construction, and home decor industries with online item purchase, vendor search, detection of fraud, verification of vendors, interior designers and police services search, inquiry help, advertising, quotation, exhibition, and job applications. According to the app maturity and productiveness analysis results, over 63 percent of the evaluators dispatched palpable comments regarding the suggested application.},
  keywords={Deep learning;Law enforcement;Generative AI;Customer services;Buildings;Knowledge graphs;Ceramics;Mobile applications;Fraud;Business;Flutter;Ceramics;Mobile Apps;Building Construction;Home Decor;Vendor Searches},
  doi={10.1109/ICCPCT65132.2025.11176626},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{11190176,
  author={Ghimire, Bimal and Rawat, Danda B. and Siwakoti, Yuba R.},
  booktitle={2025 International Conference on Metaverse Computing, Networking and Applications (MetaCom)}, 
  title={Blockchain and Smart Contract-Based Framework for Crowdsourcing Computation in Web 3.0}, 
  year={2025},
  volume={},
  number={},
  pages={421-429},
  abstract={The increasing complexity of computational problems across many scientific and technological domains often challenges traditional centralized computing resources. As the internet evolves toward Web 3.0, blockchain technology is emerging as a foundational infrastructure for decentralization, transparency, and distributed collaboration. Applications like the metaverse, which demand real-time responsiveness and high computational throughput, further underscore the need for scalable and resilient computing frameworks. In this regard, we propose a novel framework for crowdsourcing computationally intensive tasks using blockchain and smart contracts. Operating atop existing blockchain networks, a Master (or Requester) node defines a computational problem, decomposes it into subtasks, and deploys a smart contract to manage task distribution. Worker nodes perform the computations off-chain using local resources and submit their results via on-chain transactions. The smart contract aggregates these results and finally, the Master validates them and automatically distributes rewards in cryptocurrency. A proof-of-concept simulation using Ganache, Solidity, and off-chain scripts demonstrates the feasibility of this approach, showcasing key features such as task decomposition, off-chain computation, and automated result handling. These findings underscore blockchain’s potential to enable transparent, automated, and scalable coordination of distributed computing tasks.},
  keywords={Semantic Web;Crowdsourcing;Metaverse;Computational modeling;Smart contracts;Throughput;Real-time systems;Blockchains;Cryptocurrency;Distributed computing;Web 3.0;Blockchain technology;Smart contract;Crowdsourcing;Off-chain computation;On-chain transaction},
  doi={10.1109/MetaCom65502.2025.00069},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{11166757,
  author={Sitanggang, Tantri Wenny and Kamar, Karnawi and Priyono, Hary and Rahman, Abdul Wahab Abdul and Nusantoro, Hardjanto and Sunengsih, Meriyana},
  booktitle={2025 4th International Conference on Creative Communication and Innovative Technology (ICCIT)}, 
  title={Enhancing Educational Data Integrity Using Blockchain and Ontologies}, 
  year={2025},
  volume={},
  number={},
  pages={1-7},
  abstract={The growing need for secure and transparent data management in educational institutions has brought attention to Blockchain technology. This study explores the implementation of Blockchain for educational data integrity, focusing on how decentralized ledger technologies (DLTs) and Merkle root methods enhance trust, transparency, and security in academic environments. By integrating an ontology-based framework, this research provides a structured approach to organizing and verifying digital credentials, student records, and institutional data. The ontology aids in reducing conceptual ambiguity and supports interoperability among diverse educational technologies. Through technical analysis of Blockchain components, such as Merkle trees, cryptographic hashes, and block structures, the study demonstrates how Blockchain ensures tamper-proof and verifiable data transactions. Case studies from universities applying Blockchain-Based certification further highlight the feasibility and challenges of adoption, including infrastructure readiness, stakeholder resistance, and integration with legacy systems. The findings suggest that combining Blockchain with emerging technologies like AI and cloud computing can establish a resilient, scalable, and secure educational ecosystem. This paper contributes a theoretical and practical perspective for institutions aiming to modernize academic record keeping while addressing privacy, authenticity, and long-term data sustainability.},
  keywords={Resistance;Privacy;Data integrity;Education;Ontologies;Blockchains;Stakeholders;Sustainable development;Interoperability;Certification;Blockchain Education;Decentralized Learning;Transparency;Merkle Root;Ontology},
  doi={10.1109/ICCIT65724.2025.11166757},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{11158733,
  author={Jakku, Praveen Chaitanya and Chippagiri, Srinivas and Uppalapati, NagaSatyanarayana Raju and Dasi, Ugandhar},
  booktitle={2025 International Conference on Computing Technologies & Data Communication (ICCTDC)}, 
  title={Multi-Cloud Semantic Orchestration of AI-Driven Pipelines for Privacy-First LLM Inference}, 
  year={2025},
  volume={},
  number={},
  pages={1-8},
  abstract={Because more people need scalable and private Large Language Model (LLM) processing in the cloud, better orchestration methods for using different cloud resources are required. This research proposes a new architecture for handling Multi-Cloud Semantic Orchestration of AI-driven data pipelines to make LLM inference workflows more efficient, secure and in line with company policies. The framework makes it possible to effectively assign tasks and control data with the help of ontology-based knowledge graphs and added metadata by semantic enrichment. Our system uses distributed coordination engines, ensures both differential privacy and secure multiparty computation and selects appropriate models for proper latency, throughput and compliance. Experimental findings from using well-known NLP datasets show that the orchestration strategy proposed here reduces latency by up to 38 % and ensures more privacy by 22 % than standard single-cloud systems. This research supports better LLM operations (LLMOps) in areas like healthcare and finance, since both need to pay close attention to data protection and privacy laws.},
  keywords={Large language models;Semantics;Pipelines;Finance;Medical services;Knowledge graphs;Metadata;Throughput;Standards;Engines;multi-cloud orchestration;large language models;privacy-preserving AI;semantic pipelines;LLMOps;federated inference;secure computation},
  doi={10.1109/ICCTDC64446.2025.11158733},
  ISSN={},
  month={July},}@INPROCEEDINGS{11139896,
  author={Yalla, Swathi Tejah and Batra, Isha and Malik, Arun},
  booktitle={2025 6th International Conference for Emerging Technology (INCET)}, 
  title={Security Ontologies in Cloud Computing}, 
  year={2025},
  volume={},
  number={},
  pages={1-8},
  abstract={Cloud computing (CC) has the potential to augment collaboration, scalability, and accessibility, while providing cost-saving opportunities via enhanced and efficient computing, thus enabling enterprises to maintain focus. Cloud-based security ontologies are organized frameworks that delineate and classify topics pertinent to security in CC settings. They assist in standardizing language and enhance communication, interoperability, and comprehension among diverse stakeholders, including security experts, developers, and organizations. The ontology-based security strategy is one of the security solutions used by relevant parties. Nevertheless, target customers and cloud service providers see it as complex and lacking in security, since the applicable paradigms for various contexts are not clearly defined. Previous research has identified many security ideas that service providers might use, employing several evaluation methodologies. This highlights the growing need for reviewing the literature on Cloud Security Ontology (CSO). This research outlines the current state of the art in cloud computing ontology (CCO) and conducts a comprehensive literature review on the topic. Using the systematic literature review (SLR) approach, we identified 100 papers; 60 were selected based on established criteria, and 25 were assessed to be pertinent to the study. Scholars have shown active interest in four main areas of CC—service description, cloud security (CS), cloud resource management, cloud interoperability, and cloud service discovery—where cloud ontologies have had a significant impact. According to the research, fifteen of the thirty-one methods described in the literature for using ontologies in the cloud domain are still in use in legacy computing settings. The findings reveal several issues with improving company operations in cloud and multi-cloud settings via the use of ontologies. This paper delineates unsolved difficulties and potential future paths for academics in cloud ontology.},
  keywords={Cloud computing;Accuracy;Cloud computing security;Standards organizations;Training data;Ontologies;Security;Interoperability;Systematic literature review;Service level agreements;Security Ontologies;Cloud Computing;Comparative Study;Cloud Security Ontologies},
  doi={10.1109/INCET64471.2025.11139896},
  ISSN={2996-4490},
  month={May},}@ARTICLE{11142641,
  author={Liu, Gang and Zhao, Siyuan and Sumbal, Muhammad Saleem},
  journal={International Journal of Crowd Science}, 
  title={Bibliometric Analysis on Digital Talent Cultivation in China}, 
  year={2025},
  volume={9},
  number={3},
  pages={190-198},
  abstract={With the vigorous development of the digital economy and the deepening of enterprise digital transformation in China, cultivating digital talent has become a focus of academic research. This study examines the literature related to digital talent from 2015 to 2025 in the China National Knowledge Infrastructure (CNKI) journal database and employs a bibliometric analysis approach to investigate the research focus and trends in digital talent development. The analysis covers authors, institutions, keywords, research hotspots, and trend evolution. The findings reveal the following: First, with the advancement of digital transformation policies by governments and enterprises, the number of publications related to digital talent has significantly increased in recent years. Second, a stable core group of authors has yet to emerge domestically, and collaborations between research institutions are mostly confined to specific regions with limited collaboration. Third, digital talent research mainly focuses on the digital economy and digitalization, enterprise digitalization and digital transformation, and the cultivation of digital talent. Finally, on the basis of the analysis results of the knowledge graph and the national situation, implementation strategies for digital talent cultivation are proposed. These strategies inherently align with crowd science principles, where human-machine-object intelligence interactions drive collective evolution, collaborative innovation, and decentralized decision-making to enhance socio-economic efficacy.},
  keywords={Technological innovation;Systematics;Digital transformation;Digital economy;Decision making;Bibliometrics;Collaboration;Market research;Artificial intelligence;Socioeconomics;enterprise digital transformation;digital talent cultivation;digital talent strategy development;crowd science},
  doi={10.26599/IJCS.2025.9100003},
  ISSN={2398-7294},
  month={August},}@ARTICLE{11119790,
  author={Xie, Xiang and Liu, Jianxun and Cao, Buqing and Zhao, Wenyu and Lin, Sheng and Shi, Min and Chen, Jinjun},
  journal={IEEE Transactions on Services Computing}, 
  title={DiffMSR: A Multi-Semantic Graph Diffusion Model for Service Recommendation}, 
  year={2025},
  volume={18},
  number={5},
  pages={2740-2753},
  abstract={With the rapid development of cloud computing and service computing, service recommendation systems play a crucial role in helping users efficiently filter the appropriate services. However, the sparsity of service data and the presence of noise in interactions make it extremely challenging to accurately capture user preferences. Existing service recommendation methods based on Graph Neural Networks (GNNs) primarily rely on ID aggregation, often neglecting the richness of textual semantics and are susceptible to interaction noise, resulting in suboptimal modeling of user-service relationships. Although Large Language Models (LLMs) demonstrate remarkable advantages in capturing textual semantics, current methods struggle to effectively align structural representations with textual representations, limiting improvements in recommendation performance. To address these challenges, we propose an innovative multi-semantic graph diffusion model for service recommendation, DiffMSR, which aims to align textual and structural representations while learning the generation process of interaction graphs in a denoising manner. This approach mitigates data sparsity and effectively reduces noise interference. Specifically, the model leverages LLMs to capture the textual semantic features of service descriptions and integrates them with structured semantic information from knowledge graphs. Through cross-semantic contrastive learning, it achieves heterogeneous semantic alignment. Furthermore, the model introduces a multisemantic diffusion-based generation framework, which iteratively denoises to construct high-quality user-service interaction graphs. This significantly enhances the multi-semantic awareness of user representations, thereby improving recommendation performance. Experiments on public service datasets demonstrate that DiffMSR outperforms existing state-of-the-art baseline methods, achieving improvements of 4.13% and 6.37% in recommendation accuracy and recall, respectively.},
  keywords={Semantics;Noise;Knowledge graphs;Contrastive learning;Accuracy;Recommender systems;Noise reduction;Large language models;Feature extraction;Data models;LLM;knowledge graph;graph diffusion;service recommendation;semantic alignment},
  doi={10.1109/TSC.2025.3596891},
  ISSN={1939-1374},
  month={Sep.},}@INPROCEEDINGS{11091940,
  author={Liu, Jinsong and Dou, Keqin and Zhou, Yong and Tang, Yiqiang and Qu, Jianhong},
  booktitle={2025 The 16th International Conference on Mechanical and Intelligent Manufacturing Technologies (ICMIMT)}, 
  title={Research on Manufacturing Resources Optimization Selection Method Based on Industrial Internet Platform}, 
  year={2025},
  volume={},
  number={},
  pages={237-242},
  abstract={The industrial Internet platform serves as a crucial medium for supporting the ubiquitous connection, flexible supply, and efficient allocation of manufacturing resources, thereby facilitating the intelligent, networked, and service-oriented transformation and upgrading of the manufacturing industry. Currently, the scale of manufacturing services based on industrial Internet platforms is experiencing rapid growth, with an increasing aggregation of resources and a progressively complex interaction between resources and services. This complexity poses significant challenges for the precise matching of manufacturing resources with manufacturing service demands on industrial Internet platform. To address these issues, this paper proposes an ontology modeling framework for the supply and demand matching of manufacturing services and resources, which achieving a unified semantic description of heterogeneous resources and manufacturing services on the industrial Internet platform with Domain-Specific Language (DSL). Employing reasoning engine to facilitate the rapid matching of manufacturing services with manufacturing resources. Building upon this foundation, an ontology-assisted system is developed to provide a reference for the intelligent supply and demand matching applications of manufacturing resources in the industrial sector.},
  keywords={Adaptation models;Supply and demand;Semantics;Ontologies;Cognition;Manufacturing;Internet;DSL;Optimization;Engines;Industrial Internet Platform;manufacturing resources optimization selection method;ontology modeling;supplydemand matching},
  doi={10.1109/ICMIMT65123.2025.11091940},
  ISSN={2694-3182},
  month={May},}@INPROCEEDINGS{11052053,
  author={Shukla, Varsha and Pradhan, Rahul},
  booktitle={2025 International Conference on Intelligent and Cloud Computing (ICoICC)}, 
  title={Protein Matching for Function Prediction using Siamese Neural Network}, 
  year={2025},
  volume={},
  number={},
  pages={1-5},
  abstract={Protein similarity lies at the heart of many critical tasks in bio-informatics, from predicting protein function to drug discovery and evolutionary studies. The discipline has deployed traditional approaches such as sequence alignment and structural comparison to serve the field for decades, their limitations in scalability, sensitivity to remote homologs, and reliance on handcrafted features have become increasingly apparent in the era of high-throughput data. In this study, one of the concept of deep learning, specifically Siamese neural networks for similarity matching, is explored for protein matching. By leveraging pretrained protein sequence embeddings and biologically informed functional annotations, a framework is developed that can learn nuanced relationships between protein pairs.},
  keywords={Heart;Deep learning;Cloud computing;Sensitivity;Scalability;Graph neural networks;Protein sequence;Biology;Drug discovery;Convolutional neural networks;Siamese neural network (SNN);Convolutional neural network (CNN);Graph neural network (GNN)},
  doi={10.1109/ICoICC64033.2025.11052053},
  ISSN={},
  month={May},}@INPROCEEDINGS{11030544,
  author={Zhai, Weitong},
  booktitle={2025 10th International Conference on Cloud Computing and Big Data Analytics (ICCCBDA)}, 
  title={TSKD: A Teacher-Student Model for Session-Based Recommendation Using Knowledge Distillation}, 
  year={2025},
  volume={},
  number={},
  pages={1-6},
  abstract={Session-based recommendation (SBR) aims to predict the next interaction of anonymous users based on their limited historical behaviour. However, most existing SBR methods still suffer from data sparsity issues due to short-term interactions. To alleviate the above issues, we propose a teacher and student distillation framework called TSKD for SBR. TSKD characterizes user intent by building multiple graphs. Specifically, a student graph (cross-session graph) and a teacher graph (item attribute similarity graph) are first constructed to supplement the session data. Then, graph neural networks (GNNs) and knowledge distillation (KG) are combined to leverage multiple graph structures for self-ensemble graph learning, effectively utilizing and learning item relationships from the student and teacher model. Furthermore, a multiple graph consistency constraint is introduced to align the student and teacher graphs in the feature embedding space, which enables the student model to learn more effectively from the teacher model by incorporating additional item relationships, leading to improved performance. Extensive experiments conducted on real-world datasets demonstrate the effectiveness of TSKD.},
  keywords={Knowledge engineering;Analytical models;Cloud computing;Computational modeling;Stacking;Buildings;Knowledge graphs;Data models;Graph neural networks;Data mining;session-based recommendation;knowledge distillation;teacher-student distillation;graph neural networks;cross-session},
  doi={10.1109/ICCCBDA64898.2025.11030544},
  ISSN={2832-3734},
  month={April},}@INPROCEEDINGS{11019821,
  author={Dongre, Chandana and Bhattacharyya, Somanshu and Kanmani, Clara},
  booktitle={2025 International Conference on Next Generation Communication & Information Processing (INCIP)}, 
  title={Enhancing Cloud Security: An Ontology-Based NIDS with Semantic Technologies for VM Attack Detection}, 
  year={2025},
  volume={},
  number={},
  pages={666-671},
  abstract={In this paper, we have explored and classified different vulnerabilities in the Virtual Machine (VM) based Cloud platforms in today's world. To understand the semantic relations between various intrusions and network attacks in a Network Intrusion Detection System (NIDS), we use Protégé software to build an ontology. Additionally, the ontology-driven knowledge graph (KG) aids in continuously improving the detection capabilities by mapping these attacks. Our cloud-based NIDS can isolate the attacks detected in the network using Snort, Suricata, and Zeek tools and mapped with the proposed Ontology, focusing on Cross-VM Attacks and VM Foot Printing. Ontologies helps organize knowledge of a particular in a formal way. It is done by describing the concepts and the relationships between them. After the knowledge is organized, knowledge management systems can be used to find, connect, display, and even create new knowledge [4]. It uses semantic analysis to explain the information in a way that shows how different pieces of information are related to each other [4]. RDF validation ensures the structural integrity of our ontology, while advanced SPARQL queries are applied for constraint validation and to optimize the analysis of relationships between attack vectors. By implementing an ontology-based intrusion system with semantic technologies, we improve the effectiveness of detecting and isolating VM-based attacks in cloud environments.},
  keywords={Printing;Network intrusion detection;Knowledge graphs;Ontologies;Virtual machines;Vectors;Software;Resource description framework;Knowledge management;Semantic technology;Virtual Machine Based Cloud Platforms;Network Intrusion Detection System;Ontology;Cross VM Attacks;VM Foot Printing;Knowledge Graph},
  doi={10.1109/INCIP64058.2025.11019821},
  ISSN={},
  month={Jan},}@INPROCEEDINGS{11014969,
  author={Friedenberger, Dirk and Pirl, Lukas and Boockmeyer, Arne and Schmid, Robert and Polze, Andreas},
  booktitle={2025 IEEE 22nd International Conference on Software Architecture Companion (ICSA-C)}, 
  title={A Train Dispatcher in the Cloud Generated from RDF Models}, 
  year={2025},
  volume={},
  number={},
  pages={111-119},
  abstract={In the context of fast systems and software development, combining model-based systems engineering with automated code generation is essential for managing complexity while enhancing efficiency and adaptability. This paper presents a model-based approach, where a composite RDF model is created, which serves as a flexible basis for the subsequent generation of various artifacts. The artifacts include not only source code but also technical configuration files (e.g. Dockerfiles, Kubernetes objects), CI/CD pipeline configurations, and documentation. The approach has been successfully applied to the Train Dispatcher in the Cloud (ZLiC), a cloud-based approach to digitalize the German Zugleitbetrieb. An iterative development process enabled continuous system expansion and adaptation to specific project requirements. The generated prototype has been validated through simulations and field tests, confirming the robustness and practical applicability of the approach.},
  keywords={Adaptation models;Codes;Source coding;Pipelines;Prototypes;Documentation;Computer architecture;Resource description framework;Iterative methods;Modeling;Model-based systems engineering;ontology-based systems engineering;code generation;railway},
  doi={10.1109/ICSA-C65153.2025.00023},
  ISSN={2768-4288},
  month={March},}@INPROCEEDINGS{10942645,
  author={Sabour, Sasan and Ebrahimzadeh, Amin and Soualhia, Mbarka and Wuhib, Fetahi and Glitho, Roch. H.},
  booktitle={2025 28th Conference on Innovation in Clouds, Internet and Networks (ICIN)}, 
  title={Intent-Based Service Graph Selection for Cost-Effective Cloud Deployment}, 
  year={2025},
  volume={},
  number={},
  pages={140-147},
  abstract={Cloud computing offers a diverse range of virtualized services for users. However, selecting and composing these services to generate optimal service graphs that meet user requirements is complex because it requires an expert with extensive knowledge of the cloud applications and their service composition, to manually generate service graphs. This, however, cannot often guarantee that all the requirements are met. A potentially promising approach to address this complexity is to develop a system that enables cloud users to specify their intent and then to automatically select a service graph that meets all their requirements. In this paper, we address the problem of service graph selection while considering both functional and non-functional requirements derived from user intent. Our objective is to minimize the total deployment cost of the service graph in a data center network and to determine its placement within the distributed data center network. We formulate the problem as an Integer Linear Programming (ILP), taking into account the bandwidth and latency requirements of user intent. To solve this, we propose our Service Graph Selection (SGS) solution, which aims to achieve a near-optimal solution in a computationally efficient manner. Our results demonstrate that the proposed solution achieves a deployment cost that is only 4–6% larger than the lower bound of the optimal deployment cost.},
  keywords={Cloud computing;Data centers;Technological innovation;Lower bound;Costs;Bandwidth;Integer linear programming;Computational efficiency;Complexity theory;Cloud Computing;Cloud Services;Intent Requirements;Service Catalog;Service Graphs Selection},
  doi={10.1109/ICIN64016.2025.10942645},
  ISSN={2472-8144},
  month={March},}@INPROCEEDINGS{10933265,
  author={J, Aarav Kannan and Amshavalli, R.S. and D, Venkateswara Reddy and Varma, Vegesna Surya Vinay and Kavitha, G.},
  booktitle={2025 4th International Conference on Sentiment Analysis and Deep Learning (ICSADL)}, 
  title={A Semantic Driven Framework for Energy Efficient Task Scheduling in Dynamic Cloud Environment}, 
  year={2025},
  volume={},
  number={},
  pages={274-281},
  abstract={The rapid expansion of cloud computing has led to increased energy consumption, raising concerns about its environmental impact. Despite advancements in scheduling algorithms, existing methods face critical challenges, including suboptimal energy efficiency, limited adaptability to dynamic cloud environments, and insufficient context-aware decision-making. Recent techniques, such as Meta Reinforcement Learning, Genetic Algorithms, and Energy-Aware Scheduling, show potential but often struggle with high computational complexity, static resource allocation, or scalability issues. These shortcomings result in inefficient resource utilization, elevated energy usage, and difficulties in meeting Service Level Agreement (SLA) requirements. To address these challenges, this study introduces the Energy Efficient Semantic Scheduling Framework (ESSF), a novel approach combining semantic reasoning and energy profiling to optimize task scheduling in dynamic cloud environments. The framework integrates three key components: (1) Dynamic Energy Profiling (DEP) for real-time energy monitoring, (2) Semantic Resource Matching (SRM) utilizing knowledge graphs for context-aware allocation, and (3) Adaptive Workload Balancing (AWB) to dynamically redistribute tasks for improved resource utilization and energy efficiency. Experiments using the Google Cluster Trace Dataset demonstrate that ESSF reduces energy consumption by up to 25% compared to traditional methods while achieving 90% resource utilization and 97% SLA compliance. These results highlight the potential of the proposed framework to overcome existing limitations, offering a scalable, adaptive, and environmentally sustainable solution for task scheduling in cloud computing.},
  keywords={Cloud computing;Energy consumption;Processor scheduling;Semantics;Knowledge graphs;Dynamic scheduling;Energy efficiency;Real-time systems;Resource management;Service level agreements;Energy Efficiency;Cloud Computing;Semantic Scheduling;Dynamic Energy Profiling;Knowledge Graphs;Google Cluster Trace Dataset},
  doi={10.1109/ICSADL65848.2025.10933265},
  ISSN={},
  month={Feb},}@ARTICLE{10904297,
  author={Hosseini, Ali M. and Kastner, Wolfgang and Sauter, Thilo},
  journal={IEEE Open Journal of the Industrial Electronics Society}, 
  title={Leveraging LLMs and Knowledge Graphs to Design Secure Automation Systems}, 
  year={2025},
  volume={6},
  number={},
  pages={380-395},
  abstract={The digital transformation of Industrial Control Systems (ICSs) within the Industry 4.0 paradigm is essential for industrial organizations to remain competitive, while cybersecurity is an enabler. However, security measures, often implemented late in the engineering process, lead to costly and complicated implementations. Thus, this article is concerned with the “security by design” principle in ICSs and facilitates compliance with ICS security standards, which can be legally mandated for some critical systems or adopted by asset owners to protect their assets. Current methods for compliance demand manual efforts from security experts, making the compliance process time-consuming and costly. To address this, we propose a framework for leveraging large language models (LLMs) combined with knowledge graphs to automate the interpretation of security requirements and system architecture as two main elements of the design phase. Our knowledge graph-augmented LLM framework converts system architectures into human natural language, enhancing the automation of various security analyses, especially those that need to handle textual requirements. The framework enables validating applicable security requirements provided by IEC 62443-3-3 (a widely-used ICS security standard) concerning system designs through a question-and-answer interface. To evaluate the framework, various questions with reference responses from human experts were prepared in the context of a use case, and the quality of the LLMs' responses was measured across various metrics. Moreover, we compared the framework with a baseline approach based on formal queries. The results show that the proposed framework effectively automates security tasks and offers a user-friendly interface accessible to nonexperts.},
  keywords={Security;Ontologies;Knowledge graphs;Computer security;IEC Standards;Systems architecture;Cyberattack;Cognition;Natural languages;Large language models;Industrial control system (ICS);security by design;knowledge graph;large language model (LLM);ontology},
  doi={10.1109/OJIES.2025.3545811},
  ISSN={2644-1284},
  month={},}@ARTICLE{10807394,
  author={Xing, Xinxin and Liu, Yizhong and Wu, Qianhong and Guan, Zhenyu and Li, Dongyu and Li, Dawei and Lu, Yuan and Susilo, Willy},
  journal={IEEE Transactions on Dependable and Secure Computing}, 
  title={Multi-Committee ABE Based Decentralized Access Control With Sharding Blockchain for Web 3.0}, 
  year={2025},
  volume={22},
  number={3},
  pages={2533-2549},
  abstract={In Web 3.0’s pursuit of a decentralized and user-autonomous network, traditional access control methods, such as central servers and weak decentralized algorithms, are insufficient regarding security, fault tolerance ability, and scalability. To solve this, we first design a decentralized multi-committee attribute-based encryption, X-ABE, to address the weak decentralization and low fault tolerance in Multi-Authority Attribute-Based Encryption (MA-ABE). X-ABE replaces MA-ABE’s fragile attribute authorities with robust attribute committees, each composed of multiple nodes. By developing dual-wrapped shares techniques, we address the increased dimensionality challenge of secret sharing while maintaining only 1 distributed key generation instance. Also, a formal security definition and proof under the partial adaptive model are given using dual system encryption. Second, X-LOCK, an X-ABE based decentralized access control utilizing consensus plus sharding, is proposed for Web 3.0, to achieve full decentralization, consistency, fault tolerance, user autonomy, and scalability. Third, X-ABE-R is proposed for attribute revocation and is demonstrated in X-LOCK-R with sharding blockchain as an immutable revocation ledger. Fourth, a formal definition and comparative analysis of X-ABE’s fault tolerance abilities are demonstrated, covering aspects of liveness and safety, along with the complexity analysis. Fifth, practical evaluations are conducted, demonstrating that while improving fault tolerance, the overhead remains acceptable.},
  keywords={Fault tolerant systems;Fault tolerance;Access control;Encryption;Blockchains;Semantic Web;Security;Sharding;Computer crashes;Servers;Attribute-based encryption;blockchain;decentralization;access control;fault tolerance},
  doi={10.1109/TDSC.2024.3520121},
  ISSN={1941-0018},
  month={May},}@ARTICLE{10772600,
  author={Xie, Xiang and Liu, Jianxun and Cao, Buqing and Peng, Qian and Liu, Shanpeng and Kang, Guosheng},
  journal={IEEE Transactions on Network and Service Management}, 
  title={Service Recommendation Based on Multi-Level View Contrastive Learning}, 
  year={2025},
  volume={22},
  number={2},
  pages={1690-1702},
  abstract={In the context of the rapid development of service-oriented computing and cloud computing, selecting the service that meets the user’s needs from an ever-increasing number of Web services is always challenging. Exploiting auxiliary information such as a Knowledge Graph (KG) can significantly improve the effectiveness of service recommendations. However, current KG-based service recommendation methods usually merely integrate the knowledge semantic information into the user-service interaction model, which ignores the importance of global structure and does not fully consider the in-depth learning of individual user preferences. To this end, this paper proposes a Multi-Level View Contrastive Learning for Service Recommendation (MCSR) approach to address the above challenges. In particular, unlike traditional approaches that consider only two views, we consider three views, i.e., the global structure view, the local collaboration view, and the semantic view. Specifically, the user-service graph is regarded as the collaboration view, the service-entity graph as the semantic view, and the user-service-entity graph as the structural view. By applying contrastive learning across these views at different levels, MCSR fully leverages graph features and structural information, integrating auxiliary relational semantics into user-service interaction modeling. Furthermore, recognizing that the influence of auxiliary information on interactions varies between users and services, a meta-network strategy enables adaptive, personalized knowledge transfer across views, significantly improving recommendation accuracy., since the influence of auxiliary information on interactions varies between users and services, a meta-network strategy enables adaptive, personalized knowledge transfer across views, significantly enhancing recommendation accuracy. The experimental results show that MCSR significantly outperforms current state-of-the-art methods, with the positive impact of its key components on the recommended performance verified by ablation experiments.},
