  month={Dec},}@INPROCEEDINGS{10824600,
  author={Li, Gege and Liu, Yong and Deng, Wei and Jia, Yafei and Zhang, Aoqi and Qi, Difan},
  booktitle={2024 3rd International Conference on Cloud Computing, Big Data Application and Software Engineering (CBASE)}, 
  title={Multi Feature Deep Fusion Based on Relationship Path Enhancement for Multi-Modal Knowledge Graph Completion}, 
  year={2024},
  volume={},
  number={},
  pages={548-556},
  abstract={Multimodal knowledge graph integrates information from different modalities to enhance knowledge representation, but multimodal knowledge graphs still have deficiencies in completeness and accuracy. Most existing completion methods rely on entity text, images, and triplet information, ignoring the relationship path information between entities. In addition, due to the heterogeneity between text and images, there is often semantic conflict, and directly fusing information from different modalities makes it difficult to accurately capture the deep associations between text and images. Therefore, we propose a multimodal knowledge graph completion method based on relationship path enhancement with multi feature deep fusion. This method identifies and strengthens key relationship paths, while also deeply mining the deep semantic connections between multiple modalities, achieving deep integration between multiple features. Finally, the model was experimentally verified on the FB15K-237 and WN18RR datasets, results showed that the model improved on most metrics, which evidence the effectiveness of the proposed method.},
  keywords={Measurement;Cloud computing;Accuracy;Computational modeling;Semantics;Knowledge graphs;Feature extraction;Big Data applications;Data mining;Software engineering;Knowledge graph completion;multimodal knowledge graph;path extraction;Cross modal knowledge alignment;multifeature fusion},
