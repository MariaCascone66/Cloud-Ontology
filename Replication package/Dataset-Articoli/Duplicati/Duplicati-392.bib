	date = {2017},
	note = {Journal Abbreviation: {DEBS} - Proc. {ACM} Int. Conf. Distributed Event-Based Syst.},
	keywords = {Cloud computing, Knowledge graphs, Recommender systems, Distributed computer systems, Data visualization, Data analytics, Software architecture, Systems engineering, Application Debugging, Event pattern, Event pattern matching, Events, Log analysis, Losses, Pattern matching, Program debugging, Publisher/subscriber, Real-world scenario, System failures, Systems Administration},
	annotation = {Export Date: 10 November 2025; Cited By: 3; Conference name: 11th {ACM} International Conference on Distributed Event-Based Systems, {DEBS} 2017; Conference location: Barcelona; Universitat Politecnica de Catalunya; Conference sponsors: {ACM} {SIGMOD}; {ACM} {SIGSOFT}},
}

