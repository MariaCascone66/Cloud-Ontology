	date = {2017},
	note = {Journal Abbreviation: {IEEE} Int. Conf. Smart Technol., {EUROCON} - Conf. Proc.},
	keywords = {Cloud Computing, Cloud computing, Security, Information management, Natural resources management, Resource allocation, Resource management, Cryptography, Access control, Security and privacy, On-demand services, Emerging technologies, Public policy, Comprehensive performance, Computer privacy, Data confidentiality, Policy, Re-encryption, Re-Encryption, Reliability analysis, Resource Management},
	annotation = {Export Date: 10 November 2025; Cited By: 1; Conference name: 17th {IEEE} International Conference on Smart Technologies, {EUROCON} 2017; Conference location: Ohrid; Metropol Lake Resort; Conference sponsors: Faculty of Computer Science and Engineering; Faculty of Electrical Engineering and {IT}; Neotel; Netcetera; Saints Cyril and Methodius University},
}

@inproceedings{labidi_ontology-based_2017,
	title = {Ontology-based {SLA} negotiation and re-negotiation for cloud computing},
	isbn = {9781538617588 ({ISBN})},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85034222332&doi=10.1109%2FWETICE.2017.24&partnerID=40&md5=44d32c0781bf2cca4c23f8e50e8a3558},
	doi = {10.1109/WETICE.2017.24},
	abstract = {Cloud computing represents a new computing approach that allows creating robust and on-demand services controlled by means of Service Level Agreements ({SLA}). These documents are generated following a process of negotiation during which the client chooses a subset of clauses among choices predefined by the provider on a 'take it or leave it' basis. However, the heterogeneity between client requests and provider offers may result in inappropriate {SLA}. In addition, the cloud service context may change over the time which causes a client requirements adjustment and therefore, the {SLA} remains unsatisfactory. In this paper, we propose a cloud {SLA} negotiation and re-negotiation approach that integrate a semantic mapping between client requests and provider offers with the aim of generating a suitable {SLA} document. Moreover, our approach includes a context-aware system to adapt the {SLA} throughout reasoning techniques and automatically ensure the re-negotiation. A prototype implementation demonstrates the feasibility and the efficiency of our approach. © 2017 Elsevier B.V., All rights reserved.},
