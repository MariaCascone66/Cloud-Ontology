	keywords = {Cloud Computing, Semantic Web, Cloud computing, Knowledge graphs, Medical computing, Attribute based access control, Digital storage, Electronic health record, Electronic Health Record ({EHR}), Cryptography, Access control, Hospital data processing, Knowledge Graph (Ontology), Cloud storages, Attribute Based Access Control ({ABAC}), Electronic document exchange, Access Broker, Attribute based encryption ({ABE}), Cloud Storage, Attribute Based Encryption ({ABE})},
