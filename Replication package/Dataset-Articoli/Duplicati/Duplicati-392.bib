	title = {Access Control for Multi-tenancy in Cloud-Based Health Information Systems},
	isbn = {9781467392990 ({ISBN})},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84962781973&doi=10.1109%2FCSCloud.2015.95&partnerID=40&md5=a133b5e49e8b6e9adb595ca3f7051848},
