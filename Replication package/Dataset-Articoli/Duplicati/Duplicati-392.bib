	keywords = {Cloud-computing, Knowledge graph, Knowledge graphs, Information management, Complex networks, Big data, Graph neural networks, Computing paradigm, Information services, Big data-centric service, Big service representation, Data centric, Graph Neural Network, Multi-view learning, Quality of big service, Service capability},
