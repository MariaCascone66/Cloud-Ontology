	date = {2017},
	note = {Journal Abbreviation: Proc. - {IEEE} Int. Conf. Enabling Technol.: Infrastruct. Collab. Enterp., {WETICE}},
	keywords = {Context- awareness, Cloud computing, Semantics, Ontology, Service Level Agreements, Context-aware systems, Re negotiations, Reasoning techniques, {SLA} negotiation, {SLA} re-negotiation, On-demand services, Context-awareness, Prototype implementations},
