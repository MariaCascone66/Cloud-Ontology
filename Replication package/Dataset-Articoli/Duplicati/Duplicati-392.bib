	keywords = {Cloud-computing, Cloud computing, Semantics, Linked data, Machine-learning, Cloud services, data mining, Data mining, Data handling, Service description, cloud service brokerage, Cloud service brokerage, Computer software portability, Data analysis tool, Data mining workflow, Data-mining services, Decision trees, Learning services, linked data, service definition, Service definition, service description},
	annotation = {Export Date: 10 November 2025; Cited By: 4; Correspondence Address: M. Parra-Royon; Department of Computer Sciences and Artificial Intelligence, {SCi}2S, {DiCITS}, {DaSCI}, Imuds, University of Granada, Granada, 18071, Spain; email: manuelparra@decsai.ugr.es},
