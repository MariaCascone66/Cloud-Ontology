	note = {Journal Abbreviation: Proc. - {IEEE} Int. Conf. Digit. Health, {ICDH}},
	keywords = {Cloud Computing, Cloud-computing, Semantic Web, Knowledge graph, Knowledge graphs, Searchable encryptions, Cloud-based, Cloud securities, Ontology's, Knowledge graph (ontology), Attribute Revocation, Cloud Security, Cryptography, Search engines, Access control, Attribute revocation, Knowledge Graph (Ontology), Records management, {eHealth}, Electronic document exchange, Cloud data security, Electronic Health Record, Patient record, Searchable Encryption, Semantic access},
