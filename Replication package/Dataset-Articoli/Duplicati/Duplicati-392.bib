	keywords = {Cloud computing, Ontology, Distributed database systems, Cloud architectures, Chains, Trusted computing, Hardware, Reconfigurable hardware, Compliance, Conceptual approaches, Control instruments, Problematic issues, Regulation, Trusted Cloud, Trusted clouds, Vulnerability to compromise},
	annotation = {Export Date: 10 November 2025; Cited By: 1; Conference name: 8th {IEEE}/{ACM} International Conference on Utility and Cloud Computing, {UCC} 2015; Conference location: Limassol; Conference sponsors: Association for Computing Machinery ({ACM}); {IEEE} Computer Society},
