	keywords = {data privacy, Computation theory, Software as a service ({SaaS}), Data privacy, Privacy disclosures, Data description, Formal languages, Personal information, Computer circuits, Dynamic description logic, Interaction process, privacy enforcement, Privacy enforcement, Privacy requirements, privacy supervising, {SaaS} service, transparent interaction},
	annotation = {Export Date: 10 November 2025; Cited By: 4; Correspondence Address: C. Ke; School of Computer Science and Technology, Nanjing University of Posts and Telecommunications, Nanjing, 210003, China; email: brobo.ke@njupt.edu.cn},
