	keywords = {Cloud computing, Graph theory, Computer architecture, {RDF}, {SPARQL}, Java, Engines, Resource description framework, Servers, Partitioning algorithms, Database management, Distributed databases, Graph databases},
}

@article{d_n_ganapathy_semantically_2023,
