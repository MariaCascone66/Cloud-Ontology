	isbn = {9781509016105 ({ISBN})},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85013276120&doi=10.1109%2FBIBM.2016.7822615&partnerID=40&md5=702ea6c20d594ae2941641806489b37f},
