@inproceedings{sun_assisting_2017,
	title = {Assisting Open Education Resource Providers and Instructors to Deal with Cold Start Problem in Adaptive Micro Learning: A Service Oriented Solution},
	isbn = {9781538620052 ({ISBN})},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85032357752&doi=10.1109%2FSCC.2017.32&partnerID=40&md5=686cc854fa9527555cd4e44c1921cae2},
	doi = {10.1109/SCC.2017.32},
	abstract = {Various prior studies have leveraged cloud computing and big data techniques to promote adaptive micro open learning. However, this novel way of open education resource ({OER}) delivery and access suffers from the cold start problem of learner information. In this paper, we introduce a service oriented solution to assist {OER} providers and instructors to deal with the sparsity of data in {OER} recommendation using an ontological approach. Learners' features are predicted by spreading activation and demographic similarity based inference. An evolutionary algorithm is provided to realize the {OER} recommendation in terms of heuristic rules. © 2017 Elsevier B.V., All rights reserved.},
	pages = {196--203},
	booktitle = {Proc. - {IEEE} Int. Conf. Serv. Comput., {SCC}},
