	keywords = {Semantics, Knowledge based systems, Web services, Distributed knowledge, Ecosystems, Websites, Runtime self-adaptation, Self adaptation, Service ecosystem, Service ecosystems, Service evolution, Service evolutions, Stability, Stability evaluation},
	annotation = {Export Date: 10 November 2025; Cited By: 14; Conference name: 25th {IEEE} International Conference on Web Services, {ICWS} 2018; Conference location: San Francisco; {CA}; Conference sponsors: {IEEE}; {IEEE} Computer Society},
}

@inproceedings{erascu_architecture_2018,
	title = {An architecture for a management agency for cloud resources},
	isbn = {9781728106250 ({ISBN})},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85069533188&doi=10.1109%2FSYNASC.2018.00052&partnerID=40&md5=b8a9bb24bc2a1665d4083352b05ce231},
	doi = {10.1109/SYNASC.2018.00052},
	abstract = {Cloud computing offers attractive options to migrate corporate applications without the end users needing to manage any physical resources. While this 'ease' is appealing, several issues arise: 1) Which Cloud Providers ({CPs}) offer the best infrastructure at a fair budget? 2) I am no Cloud expert, then what are the characteristics of the infrastructure which best fit my application? To answer these questions, one must solve a resource management problem, that is, the allocation of computing, storage, networking and (indirectly) cost resources to a set of applications such that the performance objectives of the application are fulfilled. There are many approaches which answer separately these questions but there is no comprehensive and easily usable solution for these issues. {MANeUveR} solves them by integrating the following components: 1) a Web User Interface offers the end user the possibility to describe his application in terms of interactions between components and their software and hardware requirements, 2) an Offers Management System, through a crawler, periodically updates an ontology with infrastructure and services details from different {CPs}, 3) a Recommendation Engine provides a (sub) optimal solution for application deployment in the {CP} infrastructure regarding the leasing price of the virtual machines needed for deployment and their characteristics. Using a secure-billing e-mail service, we demonstrate the effectiveness of our solution. © 2020 Elsevier B.V., All rights reserved.},
	pages = {288--295},
	booktitle = {Proc. - Int. Symp. Symb. Numer. Algorithms Sci. Comput., {SYNASC}},
	publisher = {Institute of Electrical and Electronics Engineers Inc.},
	author = {Eraşcu, M. and Iuhasz, G. and Micota, F.},
	date = {2018},
	note = {Journal Abbreviation: Proc. - Int. Symp. Symb. Numer. Algorithms Sci. Comput., {SYNASC}},
	keywords = {Cloud Computing, Application programs, Cloud computing, Ontology, Optimization, User interfaces, Management systems, Application deployment, Software and hardwares, Budget control, Crawling, Meta heuristics, Metaheuristics, Performance objective, Resource management problems, {SMT} solving, Web user interface},
	annotation = {Export Date: 10 November 2025; Cited By: 2; Conference name: 20th International Symposium on Symbolic and Numeric Algorithms for Scientific Computing, {SYNASC} 2018; Conference location: Timisoara},
}

