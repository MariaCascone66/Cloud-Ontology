@article{wang_integrated_2017,
	title = {An integrated industrial ethernet solution for the implementation of smart factory},
	volume = {5},
	issn = {21693536 ({ISSN})},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85033684570&doi=10.1109%2FACCESS.2017.2770180&partnerID=40&md5=7d4a3e94b98266b6f4f06435df651d3d},
