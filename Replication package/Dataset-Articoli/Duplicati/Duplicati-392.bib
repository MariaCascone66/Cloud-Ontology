	editor = {Anderst-Kotsis, G. and Steinbauer, M. and Khalil, I. and Indrawan-Santiago, M. and Salvadori, I.L.},
	date = {2017},
	note = {Journal Abbreviation: {ACM} Int. Conf. Proc. Ser.},
	keywords = {Semantic Web, Authentication, Distributed computer systems, Knowledge based systems, Information management, Web services, Information retrieval, Cryptography, Search engines, Semantic web, Websites, Security and privacy protection, {OWL}, Semantic search engines, Information analysis, {RDF}, Authorization, Security requirements, Knowledge information, Knowledge resource, Personally identifiable information, Security vulnerabilities},
