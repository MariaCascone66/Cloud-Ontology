	isbn = {23302186 ({ISSN}); 23302194 ({ISSN}); 9798350339826 ({ISBN}); 9781728150116 ({ISBN}); 9781538678992 ({ISBN}); 9781509014453 ({ISBN}); 9798331507589 ({ISBN}); 9781479940936 ({ISBN}); 9780738143767 ({ISBN}); 9781665463676 ({ISBN}); 9781538606926 ({ISBN}); 9780768550954 ({ISBN})},
