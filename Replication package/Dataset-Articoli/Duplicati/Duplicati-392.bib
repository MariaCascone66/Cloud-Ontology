	keywords = {Cloud-computing, Semantic Web, Semantic-Web, Cloud computing architecture, {OWL}, Fog computing, Computing architecture, Congestion control (communication), {EDGE} Networks, Internet-of-car, Internet-of-Cars, Internet-of-vehicle, Internet-of-Vehicles, Low latency, Pub/sub, Pub/Sub system, Pub/sub systems, Traffic congestion},
