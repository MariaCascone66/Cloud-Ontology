@article{xie_service_2025,
	title = {Service Recommendation Based on Multi-Level View Contrastive Learning},
	volume = {22},
	issn = {19324537 ({ISSN})},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85211043256&doi=10.1109%2FTNSM.2024.3510399&partnerID=40&md5=37d5009ae5c4c34552e1fcd5f944c1e9},
	doi = {10.1109/TNSM.2024.3510399},
	abstract = {In the context of the rapid development of service-oriented computing and cloud computing, selecting the service that meets the user’s needs from an ever-increasing number of Web services is always challenging. Exploiting auxiliary information such as a Knowledge Graph ({KG}) can significantly improve the effectiveness of service recommendations. However, current {KG}-based service recommendation methods usually merely integrate the knowledge semantic information into the user-service interaction model, which ignores the importance of global structure and does not fully consider the in-depth learning of individual user preferences. To this end, this paper proposes a Multi-Level View Contrastive Learning for Service Recommendation ({MCSR}) approach to address the above challenges. In particular, unlike traditional approaches that consider only two views, we consider three views, i.e., the global structure view, the local collaboration view, and the semantic view. Specifically, the user-service graph is regarded as the collaboration view, the service-entity graph as the semantic view, and the user-service-entity graph as the structural view. By applying contrastive learning across these views at different levels, {MCSR} fully leverages graph features and structural information, integrating auxiliary relational semantics into user-service interaction modeling. Furthermore, recognizing that the influence of auxiliary information on interactions varies between users and services, a meta-network strategy enables adaptive, personalized knowledge transfer across views, significantly improving recommendation accuracy., since the influence of auxiliary information on interactions varies between users and services, a meta-network strategy enables adaptive, personalized knowledge transfer across views, significantly enhancing recommendation accuracy. The experimental results show that {MCSR} significantly outperforms current state-of-the-art methods, with the positive impact of its key components on the recommended performance verified by ablation experiments. © 2025 Elsevier B.V., All rights reserved.},
	pages = {1690--1702},
	number = {2},
	journaltitle = {{IEEE} Transactions on Network and Service Management},
	shortjournal = {{IEEE} Trans. Netw. Serv. Manage.},
	author = {Xie, X. and Liu, J. and Cao, B. and Peng, Q. and Liu, S. and Kang, G.},
	date = {2025},
	note = {Publisher: Institute of Electrical and Electronics Engineers Inc.},
	keywords = {Knowledge graph, Knowledge graphs, Knowledge transfer, 'current, Auxiliary information, contrastive learning, Global structure, Interaction modeling, Meta-network, meta-networks, Multi-level view, Multilevels, service recommendation, Service recommendations, User-service interaction},
	annotation = {Export Date: 10 November 2025; Cited By: 0; Correspondence Address: J. Liu; Hunan University of Science and Technology, School of Computer Science and Engineering, Hunan Key Laboratory for Service Computing and Novel Software Technology, Xiangtan, 411100, China; email: ljx529@gmail.com; B. Cao; Hunan University of Science and Technology, School of Computer Science and Engineering, Xiangtan, 411100, China; email: buqingcao@gmail.com},
}

