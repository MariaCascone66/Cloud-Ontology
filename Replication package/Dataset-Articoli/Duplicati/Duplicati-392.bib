@inproceedings{s_kouchi_multi-criteria_2024,
	title = {Multi-criteria based virtual {SaaS} Selection in Cloud Environment},
	doi = {10.1109/GCET64327.2024.10934712},
	abstract = {The process of selecting Software as a Service ({SaaS}) in Cloud Computing depends on providers’ capabilities to articulate the non-functional attributes of their {SaaS} offerings semantically and understand customer requests along with their non-functional requirements. This study conceptualizes {SaaS} selection as a multi-criteria combinatorial problem within predefined constraints. To address this, we introduce a hybrid approach incorporating an entire optimization algorithm. Our method leverages semantic and microservice (individual) {SaaS} descriptions in the Ontology Web Language for Services ({OWL}-S). Each micro {SaaS} is linked to compatible {SaaS} instances (offering similar functionality) in a unified environment, guided by the compatibility concept. A micro-{SaaS} may not satisfy a particular functional need in specific contexts. However, it is possible to meet customer requirements by combining the available {SaaS} instances, called virtual {SaaS}. The chosen {SaaS} should be the best based on the defined quality and quantitative criteria. Our proposed approach, employing multi-criteria analysis, capitalizes on a dedicated Quality of Service ({QoS}) ontology tailored for the cloud environment and web applications. Moreover, a simulation design proved that a substantial selection of {SaaS}(s) could be achieved in a cloud.},
	eventtitle = {Global Congress on Emerging Technologies ({GCET}-2024)},
	pages = {125--131},
	booktitle = {Global Congress on Emerging Technologies ({GCET}-2024)},
	author = {{S. Kouchi} and {H. Nacer} and {H. Slimani}},
	date = {2024-12-09},
	note = {Journal Abbreviation: Global Congress on Emerging Technologies ({GCET}-2024)},
	keywords = {Cloud computing, Semantics, Security, Optimization, Ontologies, Quality of service, Quality of Service ({QoS}), Multi-criteria, Selection of {SaaS} services, Semantic Annotations, Whole optimization algorithm, Software as a service, Elasticity, Complexity theory, Microservice architectures},
}

@inproceedings{w_wong_knowledge_2024,
