	note = {Journal Abbreviation: 2016 {IEEE} International Congress on Big Data ({BigData} Congress)},
	keywords = {Cloud computing, Semantics, Ontologies, Service discovery, Cloud service, Search engines, Feature extraction, Search engine},
}

