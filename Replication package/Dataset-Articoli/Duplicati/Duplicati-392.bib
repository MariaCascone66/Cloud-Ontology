@inproceedings{opara_representing_2019,
	title = {Representing cloud security and privacy policies and detecting potential problems},
	isbn = {9781728141176 ({ISBN})},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85205303904&doi=10.1109%2FDataCom.2019.00040&partnerID=40&md5=86d08effcb5b7d017b25c1296ffd3138},
	doi = {10.1109/DataCom.2019.00040},
	abstract = {Cloud computing involves multiple stakeholders, each with its own security, privacy and trust requirements, where policy enforcement is shared between the various stakeholders, in contrast to on-premise computing, where an organizations policies are enforced using internal resources. To effectively manage and integrate security requirements in the cloud, the provider and customer needs to negotiate and agree upon the applied security policies. However, there is no standard way of expressing cloud security policies in a vendor-agnostic manner. Cloud security and privacy policies are often stated in an informal and ad-hoc manner, which leads to policy conflicts, incompleteness, ambiguity and inconsistencies. In this paper, we present {CERBERUS}, a framework for representing cloud security and privacy policies and detecting potential problems in the policies. {CERBERUS} adopts an object-oriented approach and consists of an ontology and notation, policies, guidelines and rules, and a tool for capturing and detecting policy errors. Using {CERBERUS}, policies can be analyzed for potential problems, including policy conflicts, inconsistencies, ambiguities, and incompleteness. An application of {CERBERUS} shows that it indeed helps to discover policy errors, which otherwise would go undetected or in many cases would be detected a posteriori at runtime. © 2024 Elsevier B.V., All rights reserved.},
	pages = {214--221},
	booktitle = {Proc. - {IEEE} Int. Conf. Big Data Intell. Comput., {DataCom}},
	publisher = {Institute of Electrical and Electronics Engineers Inc.},
	author = {Opara, A. and Song, Y. and Cho, S.-J. and Chung, L.},
	date = {2019},
